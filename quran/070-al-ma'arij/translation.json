[
  {
    "id": 5376,
    "verse_number": 1,
    "verse_key": "70:1",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167306,
        "resource_id": 31,
        "text": "Un demandeur a réclamé un châtiment inéluctable,"
      }
    ]
  },
  {
    "id": 5377,
    "verse_number": 2,
    "verse_key": "70:2",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167313,
        "resource_id": 31,
        "text": "pour les mécréants, que nul ne pourrait repousser,"
      }
    ]
  },
  {
    "id": 5378,
    "verse_number": 3,
    "verse_key": "70:3",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167320,
        "resource_id": 31,
        "text": "et qui vient d’Allah, le Maître des voies d’ascension."
      }
    ]
  },
  {
    "id": 5379,
    "verse_number": 4,
    "verse_key": "70:4",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167332,
        "resource_id": 31,
        "text": "Les Anges ainsi que l’Esprit montent vers Lui en un jour dont la durée est de cinquante mille ans."
      }
    ]
  },
  {
    "id": 5380,
    "verse_number": 5,
    "verse_key": "70:5",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167338,
        "resource_id": 31,
        "text": "Supporte donc, d’une belle patience."
      }
    ]
  },
  {
    "id": 5381,
    "verse_number": 6,
    "verse_key": "70:6",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167339,
        "resource_id": 31,
        "text": "Ils le (le châtiment) voient bien loin,"
      }
    ]
  },
  {
    "id": 5382,
    "verse_number": 7,
    "verse_key": "70:7",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167340,
        "resource_id": 31,
        "text": "alors que Nous le voyons bien proche,"
      }
    ]
  },
  {
    "id": 5383,
    "verse_number": 8,
    "verse_key": "70:8",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167341,
        "resource_id": 31,
        "text": "le jour où le ciel sera comme du métal en fusion."
      }
    ]
  },
  {
    "id": 5384,
    "verse_number": 9,
    "verse_key": "70:9",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167342,
        "resource_id": 31,
        "text": "Et les montagnes comme de la laine,"
      }
    ]
  },
  {
    "id": 5385,
    "verse_number": 10,
    "verse_key": "70:10",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167305,
        "resource_id": 31,
        "text": "où nul ami dévoué ne s’enquerra d’un ami,"
      }
    ]
  },
  {
    "id": 5386,
    "verse_number": 11,
    "verse_key": "70:11",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167307,
        "resource_id": 31,
        "text": "bien qu’ils se voient l’un l’autre. Le criminel aimerait pouvoir se racheter du châtiment de ce jour, en livrant ses enfants,"
      }
    ]
  },
  {
    "id": 5387,
    "verse_number": 12,
    "verse_key": "70:12",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167396,
        "resource_id": 31,
        "text": "sa compagne, son frère,"
      }
    ]
  },
  {
    "id": 5388,
    "verse_number": 13,
    "verse_key": "70:13",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167348,
        "resource_id": 31,
        "text": "même son clan qui lui donnait asile,"
      }
    ]
  },
  {
    "id": 5389,
    "verse_number": 14,
    "verse_key": "70:14",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167309,
        "resource_id": 31,
        "text": "et tout ce qui est sur la terre, tout, qui pourrait le sauver."
      }
    ]
  },
  {
    "id": 5390,
    "verse_number": 15,
    "verse_key": "70:15",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 164878,
        "resource_id": 31,
        "text": "Mais rien [ne le sauvera]. [L’Enfer] est un brasier."
      }
    ]
  },
  {
    "id": 5391,
    "verse_number": 16,
    "verse_key": "70:16",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167310,
        "resource_id": 31,
        "text": "arrachant brutalement la peau du crâne."
      }
    ]
  },
  {
    "id": 5392,
    "verse_number": 17,
    "verse_key": "70:17",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167311,
        "resource_id": 31,
        "text": "Il appellera celui qui tournait le dos et s’en allait,"
      }
    ]
  },
  {
    "id": 5393,
    "verse_number": 18,
    "verse_key": "70:18",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167322,
        "resource_id": 31,
        "text": "amassait et thésaurisait."
      }
    ]
  },
  {
    "id": 5394,
    "verse_number": 19,
    "verse_key": "70:19",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167312,
        "resource_id": 31,
        "text": "Oui, l’homme a été créé instable [très inquiet] ;"
      }
    ]
  },
  {
    "id": 5395,
    "verse_number": 20,
    "verse_key": "70:20",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167314,
        "resource_id": 31,
        "text": "quand le malheur le touche, il est abattu ;"
      }
    ]
  },
  {
    "id": 5396,
    "verse_number": 21,
    "verse_key": "70:21",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167154,
        "resource_id": 31,
        "text": "et quand le bonheur le touche, il est grand refuseur."
      }
    ]
  },
  {
    "id": 5397,
    "verse_number": 22,
    "verse_key": "70:22",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 159429,
        "resource_id": 31,
        "text": "Sauf ceux qui pratiquent la prière (As-Salât)."
      }
    ]
  },
  {
    "id": 5398,
    "verse_number": 23,
    "verse_key": "70:23",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 159521,
        "resource_id": 31,
        "text": "qui sont assidu à leurs prières,"
      }
    ]
  },
  {
    "id": 5399,
    "verse_number": 24,
    "verse_key": "70:24",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167315,
        "resource_id": 31,
        "text": "et sur les bien desquels il y a un droit bien déterminé [l'aumône]"
      }
    ]
  },
  {
    "id": 5400,
    "verse_number": 25,
    "verse_key": "70:25",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 165057,
        "resource_id": 31,
        "text": "pour le mendiant et le déshérité ;"
      }
    ]
  },
  {
    "id": 5401,
    "verse_number": 26,
    "verse_key": "70:26",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167316,
        "resource_id": 31,
        "text": "et qui déclarent véridique le Jour de la Rétribution,"
      }
    ]
  },
  {
    "id": 5402,
    "verse_number": 27,
    "verse_key": "70:27",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167317,
        "resource_id": 31,
        "text": "et ceux qui craignent le châtiment de leur Seigneur."
      }
    ]
  },
  {
    "id": 5403,
    "verse_number": 28,
    "verse_key": "70:28",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167318,
        "resource_id": 31,
        "text": "Car vraiment, il n’y a nulle assurance contre le châtiment de leur Seigneur ;"
      }
    ]
  },
  {
    "id": 5404,
    "verse_number": 29,
    "verse_key": "70:29",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167319,
        "resource_id": 31,
        "text": "et qui se maintiennent dans la chasteté."
      }
    ]
  },
  {
    "id": 5405,
    "verse_number": 30,
    "verse_key": "70:30",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167321,
        "resource_id": 31,
        "text": "Et n’ont pas de rapports qu’avec leurs épouses ou les esclaves qu’ils possèdent car dans ce cas, ils ne sont pas blâmables,"
      }
    ]
  },
  {
    "id": 5406,
    "verse_number": 31,
    "verse_key": "70:31",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167324,
        "resource_id": 31,
        "text": "mais ceux qui cherchent [leur plaisir] en dehors de cela, sont des transgresseurs ;"
      }
    ]
  },
  {
    "id": 5407,
    "verse_number": 32,
    "verse_key": "70:32",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167323,
        "resource_id": 31,
        "text": "et qui gardent les dépôts confiés à eux, et respectent leurs engagements scrupuleusement."
      }
    ]
  },
  {
    "id": 5408,
    "verse_number": 33,
    "verse_key": "70:33",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167325,
        "resource_id": 31,
        "text": "Et qui témoignent de la stricte vérité,"
      }
    ]
  },
  {
    "id": 5409,
    "verse_number": 34,
    "verse_key": "70:34",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167326,
        "resource_id": 31,
        "text": "et qui sont régulier dans leur prière (As-Salât)."
      }
    ]
  },
  {
    "id": 5410,
    "verse_number": 35,
    "verse_key": "70:35",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 504,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167327,
        "resource_id": 31,
        "text": "Ceux-là seront honorés dans des Jardins."
      }
    ]
  },
  {
    "id": 5411,
    "verse_number": 36,
    "verse_key": "70:36",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 505,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167328,
        "resource_id": 31,
        "text": "Qu’ont donc, ceux qui ont mécru, à courir vers toi, le cou tendu,"
      }
    ]
  },
  {
    "id": 5412,
    "verse_number": 37,
    "verse_key": "70:37",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 505,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167329,
        "resource_id": 31,
        "text": "de droite et de gauche, [venant] par bandes ?"
      }
    ]
  },
  {
    "id": 5413,
    "verse_number": 38,
    "verse_key": "70:38",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 505,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167330,
        "resource_id": 31,
        "text": "Chacun d’eux convoite-t-il qu’on le laisse entrer au Jardin des délices ?"
      }
    ]
  },
  {
    "id": 5414,
    "verse_number": 39,
    "verse_key": "70:39",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 505,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 569,
    "juz_number": 29,
    "translations": [
      {
        "id": 167331,
        "resource_id": 31,
        "text": "Mais non ! Nous les avons créés de ce qu’ils savent . <sup foot_note=232983>1</sup>"
      }
    ]
  },
  {
    "id": 5415,
    "verse_number": 40,
    "verse_key": "70:40",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 505,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 570,
    "juz_number": 29,
    "translations": [
      {
        "id": 167333,
        "resource_id": 31,
        "text": "Eh Non !... Je jure par le Seigneur des Levants  et des Couchants que Nous sommes capables <sup foot_note=232984>1</sup>"
      }
    ]
  },
  {
    "id": 5416,
    "verse_number": 41,
    "verse_key": "70:41",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 505,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 570,
    "juz_number": 29,
    "translations": [
      {
        "id": 167334,
        "resource_id": 31,
        "text": "de les remplacer par de meilleurs qu’eux, et nul ne peut Nous en empêcher."
      }
    ]
  },
  {
    "id": 5417,
    "verse_number": 42,
    "verse_key": "70:42",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 505,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 570,
    "juz_number": 29,
    "translations": [
      {
        "id": 167335,
        "resource_id": 31,
        "text": "Laisse-les donc s’enfoncer (dans leur mécréance) et se divertir jusqu’à ce qu’ils rencontrent leur jour dont on les menaçait,"
      }
    ]
  },
  {
    "id": 5418,
    "verse_number": 43,
    "verse_key": "70:43",
    "hizb_number": 57,
    "rub_el_hizb_number": 228,
    "ruku_number": 505,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 570,
