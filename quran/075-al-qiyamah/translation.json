[
  {
    "id": 5552,
    "verse_number": 1,
    "verse_key": "75:1",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167720,
        "resource_id": 31,
        "text": "Non !... Je jure par le jour de la Résurrection!"
      }
    ]
  },
  {
    "id": 5553,
    "verse_number": 2,
    "verse_key": "75:2",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167646,
        "resource_id": 31,
        "text": "Mais non !, Je jure par l’âme qui ne cesse de se blâmer <sup foot_note=233018>1</sup>."
      }
    ]
  },
  {
    "id": 5554,
    "verse_number": 3,
    "verse_key": "75:3",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167657,
        "resource_id": 31,
        "text": "L’homme, pense-t-il que Nous ne réunirons jamais ses os ?"
      }
    ]
  },
  {
    "id": 5555,
    "verse_number": 4,
    "verse_key": "75:4",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167660,
        "resource_id": 31,
        "text": "Mais si ! Nous somme Capable de remettre à leur place les extrémités de ses doigts <sup foot_note=233019>1</sup>."
      }
    ]
  },
  {
    "id": 5556,
    "verse_number": 5,
    "verse_key": "75:5",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167662,
        "resource_id": 31,
        "text": "L’homme voudrait plutôt continuer à vivre en libertin."
      }
    ]
  },
  {
    "id": 5557,
    "verse_number": 6,
    "verse_key": "75:6",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167666,
        "resource_id": 31,
        "text": "Il interroge : \"A quand, le Jour de la Résurrection ?\""
      }
    ]
  },
  {
    "id": 5558,
    "verse_number": 7,
    "verse_key": "75:7",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167668,
        "resource_id": 31,
        "text": "Lorsque la vue sera éblouie,"
      }
    ]
  },
  {
    "id": 5559,
    "verse_number": 8,
    "verse_key": "75:8",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167670,
        "resource_id": 31,
        "text": "et que la lune s’éclipsera,"
      }
    ]
  },
  {
    "id": 5560,
    "verse_number": 9,
    "verse_key": "75:9",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167672,
        "resource_id": 31,
        "text": "et que le soleil et la lune seront réunis,"
      }
    ]
  },
  {
    "id": 5561,
    "verse_number": 10,
    "verse_key": "75:10",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167631,
        "resource_id": 31,
        "text": "l’homme, ce jour-là, dira : \"Où fuir ?\""
      }
    ]
  },
  {
    "id": 5562,
    "verse_number": 11,
    "verse_key": "75:11",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167625,
        "resource_id": 31,
        "text": "Non ! Point de refuge !"
      }
    ]
  },
  {
    "id": 5563,
    "verse_number": 12,
    "verse_key": "75:12",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167626,
        "resource_id": 31,
        "text": "Vers ton Seigneur sera, ce jour-là, le retour."
      }
    ]
  },
  {
    "id": 5564,
    "verse_number": 13,
    "verse_key": "75:13",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167627,
        "resource_id": 31,
        "text": "L’homme sera informé ce jour-là de ce qu’il aura avancé et de ce qu’il aura remis à plus tard."
      }
    ]
  },
  {
    "id": 5565,
    "verse_number": 14,
    "verse_key": "75:14",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167628,
        "resource_id": 31,
        "text": "Mais l’homme sera un témoin perspicace contre lui-même,"
      }
    ]
  },
  {
    "id": 5566,
    "verse_number": 15,
    "verse_key": "75:15",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167629,
        "resource_id": 31,
        "text": "quand même il présenterait ses excuses."
      }
    ]
  },
  {
    "id": 5567,
    "verse_number": 16,
    "verse_key": "75:16",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 162415,
        "resource_id": 31,
        "text": "Ne remue pas ta langue pour hâter sa récitation <sup foot_note=233022>1</sup> :"
      }
    ]
  },
  {
    "id": 5568,
    "verse_number": 17,
    "verse_key": "75:17",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167630,
        "resource_id": 31,
        "text": "Son rassemblement (dans ton cœur et sa fixation dans ta mémoire) Nous incombent, ainsi que la façon de le réciter <sup foot_note=233023>1</sup>."
      }
    ]
  },
  {
    "id": 5569,
    "verse_number": 18,
    "verse_key": "75:18",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167634,
        "resource_id": 31,
        "text": "Quand donc Nous le récitons <sup foot_note=233024>1</sup>, suis sa récitation."
      }
    ]
  },
  {
    "id": 5570,
    "verse_number": 19,
    "verse_key": "75:19",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167632,
        "resource_id": 31,
        "text": "A Nous, ensuite incombera son explication."
      }
    ]
  },
  {
    "id": 5571,
    "verse_number": 20,
    "verse_key": "75:20",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167635,
        "resource_id": 31,
        "text": "Mais vous aimez plutôt [la vie] éphémère,"
      }
    ]
  },
  {
    "id": 5572,
    "verse_number": 21,
    "verse_key": "75:21",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167636,
        "resource_id": 31,
        "text": "et vous délaissez l’au-delà."
      }
    ]
  },
  {
    "id": 5573,
    "verse_number": 22,
    "verse_key": "75:22",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167637,
        "resource_id": 31,
        "text": "Ce jour-là, il y aura des visages resplendissants."
      }
    ]
  },
  {
    "id": 5574,
    "verse_number": 23,
    "verse_key": "75:23",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167638,
        "resource_id": 31,
        "text": "qui regarderont leur Seigneur ;"
      }
    ]
  },
  {
    "id": 5575,
    "verse_number": 24,
    "verse_key": "75:24",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167639,
        "resource_id": 31,
        "text": "et il y aura ce jour-là, des visages assombris,"
      }
    ]
  },
  {
    "id": 5576,
    "verse_number": 25,
    "verse_key": "75:25",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167640,
        "resource_id": 31,
        "text": "qui s’attendent à subir une catastrophe."
      }
    ]
  },
  {
    "id": 5577,
    "verse_number": 26,
    "verse_key": "75:26",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167641,
        "resource_id": 31,
        "text": "Mais non ! Quand [l’âme] en arrive aux clavicules."
      }
    ]
  },
  {
    "id": 5578,
    "verse_number": 27,
    "verse_key": "75:27",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167642,
        "resource_id": 31,
        "text": "et qu’on dit : \"Qui est exorciseur ? <sup foot_note=233026>1</sup>\""
      }
    ]
  },
  {
    "id": 5579,
    "verse_number": 28,
    "verse_key": "75:28",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167643,
        "resource_id": 31,
        "text": "et qu’il [l’agonisant] est convaincu que c’est la séparation (la mort),"
      }
    ]
  },
  {
    "id": 5580,
    "verse_number": 29,
    "verse_key": "75:29",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167644,
        "resource_id": 31,
        "text": "et que la jambe s’enlace à la jambe,"
      }
    ]
  },
  {
    "id": 5581,
    "verse_number": 30,
    "verse_key": "75:30",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 514,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167647,
        "resource_id": 31,
        "text": "c’est vers ton Seigneur, ce jour-là que tu seras conduit."
      }
    ]
  },
  {
    "id": 5582,
    "verse_number": 31,
    "verse_key": "75:31",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 515,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167648,
        "resource_id": 31,
        "text": "Mais il n’a ni cru, ni fait la prière (As-Salât),"
      }
    ]
  },
  {
    "id": 5583,
    "verse_number": 32,
    "verse_key": "75:32",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 515,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167649,
        "resource_id": 31,
        "text": "par contre, il a démenti et tourné le dos,"
      }
    ]
  },
  {
    "id": 5584,
    "verse_number": 33,
    "verse_key": "75:33",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 515,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167650,
        "resource_id": 31,
        "text": "puis il s’en est allé vers sa famille, marchant avec orgueil."
      }
    ]
  },
  {
    "id": 5585,
    "verse_number": 34,
    "verse_key": "75:34",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 515,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 169413,
        "resource_id": 31,
        "text": "\"Malheur à toi, malheur !\""
      }
    ]
  },
  {
    "id": 5586,
    "verse_number": 35,
    "verse_key": "75:35",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 515,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167651,
        "resource_id": 31,
        "text": "Et encore malheur à toi, malheur !"
      }
    ]
  },
  {
    "id": 5587,
    "verse_number": 36,
    "verse_key": "75:36",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 515,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167652,
        "resource_id": 31,
        "text": "L’homme pense-t-il qu’on le laissera sans obligation à observer ? <sup foot_note=233029>1</sup>"
      }
    ]
  },
  {
    "id": 5588,
    "verse_number": 37,
    "verse_key": "75:37",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 515,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167653,
        "resource_id": 31,
        "text": "N’était-il pas une goutte de sperme éjaculé ?"
      }
    ]
  },
  {
    "id": 5589,
    "verse_number": 38,
    "verse_key": "75:38",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 515,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167654,
        "resource_id": 31,
        "text": "Et ensuite une adhérence Puis [Allah] l’a créée et formée harmonieusement;"
      }
    ]
  },
  {
    "id": 5590,
    "verse_number": 39,
    "verse_key": "75:39",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 515,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167655,
        "resource_id": 31,
        "text": "puis en a fait alors les deux éléments de couple: le mâle et la femelle ?"
      }
    ]
  },
  {
    "id": 5591,
    "verse_number": 40,
    "verse_key": "75:40",
    "hizb_number": 58,
    "rub_el_hizb_number": 231,
    "ruku_number": 515,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 578,
    "juz_number": 29,
    "translations": [
      {
        "id": 167658,
        "resource_id": 31,
        "text": "Celui-là (Allah) n’est-Il pas capable de faire revivre les morts ?<sup foot_note=233030>1</sup>"
      }
    ]
  }
]