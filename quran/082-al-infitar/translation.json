[
  {
    "id": 5830,
    "verse_number": 1,
    "verse_key": "82:1",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 165067,
        "resource_id": 31,
        "text": "Quand le ciel se rompra,"
      }
    ]
  },
  {
    "id": 5831,
    "verse_number": 2,
    "verse_key": "82:2",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167952,
        "resource_id": 31,
        "text": "et que les étoiles se disperseront,"
      }
    ]
  },
  {
    "id": 5832,
    "verse_number": 3,
    "verse_key": "82:3",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167956,
        "resource_id": 31,
        "text": "et que les mers confondront leurs eaux,"
      }
    ]
  },
  {
    "id": 5833,
    "verse_number": 4,
    "verse_key": "82:4",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167958,
        "resource_id": 31,
        "text": "et que les tombeaux seront bouleversés,"
      }
    ]
  },
  {
    "id": 5834,
    "verse_number": 5,
    "verse_key": "82:5",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167960,
        "resource_id": 31,
        "text": "toute âme saura alors ce qu’elle a accompli et ce qu’elle a remis de faire à plus tard."
      }
    ]
  },
  {
    "id": 5835,
    "verse_number": 6,
    "verse_key": "82:6",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167962,
        "resource_id": 31,
        "text": "Ô homme ! Qu’est-ce qui t’a trompé au sujet de ton Seigneur, le Noble,"
      }
    ]
  },
  {
    "id": 5836,
    "verse_number": 7,
    "verse_key": "82:7",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167964,
        "resource_id": 31,
        "text": "qui t’a créé, puis modelé et constitué harmonieusement ?"
      }
    ]
  },
  {
    "id": 5837,
    "verse_number": 8,
    "verse_key": "82:8",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167966,
        "resource_id": 31,
        "text": "Il t’a façonné dans la forme qu’Il a voulue."
      }
    ]
  },
  {
    "id": 5838,
    "verse_number": 9,
    "verse_key": "82:9",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167968,
        "resource_id": 31,
        "text": "Non...! [malgré tout] vous traitez la Rétribution de mensonge;"
      }
    ]
  },
  {
    "id": 5839,
    "verse_number": 10,
    "verse_key": "82:10",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167951,
        "resource_id": 31,
        "text": "alors que veillent sur vous des gardiens <sup foot_note=233071>1</sup>,"
      }
    ]
  },
  {
    "id": 5840,
    "verse_number": 11,
    "verse_key": "82:11",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167948,
        "resource_id": 31,
        "text": "de nobles scribes,"
      }
    ]
  },
  {
    "id": 5841,
    "verse_number": 12,
    "verse_key": "82:12",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 165607,
        "resource_id": 31,
        "text": "qui savent ce que vous faites."
      }
    ]
  },
  {
    "id": 5842,
    "verse_number": 13,
    "verse_key": "82:13",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 165642,
        "resource_id": 31,
        "text": "Les bons seront, certes, dans un [jardin] de délice,"
      }
    ]
  },
  {
    "id": 5843,
    "verse_number": 14,
    "verse_key": "82:14",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 165718,
        "resource_id": 31,
        "text": "et les libertins seront, certes, dans une fournaise,"
      }
    ]
  },
  {
    "id": 5844,
    "verse_number": 15,
    "verse_key": "82:15",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 165939,
        "resource_id": 31,
        "text": "où ils brûleront, le jour de la Rétribution,"
      }
    ]
  },
  {
    "id": 5845,
    "verse_number": 16,
    "verse_key": "82:16",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 166163,
        "resource_id": 31,
        "text": "incapables de s’en échapper."
      }
    ]
  },
  {
    "id": 5846,
    "verse_number": 17,
    "verse_key": "82:17",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 166805,
        "resource_id": 31,
        "text": "Et qui te dira ce qu’est le jour de la Rétribution ?"
      }
    ]
  },
  {
    "id": 5847,
    "verse_number": 18,
    "verse_key": "82:18",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167949,
        "resource_id": 31,
        "text": "Encore une fois, qui te dira ce qu’est le jour de la Rétribution ?"
      }
    ]
  },
  {
    "id": 5848,
    "verse_number": 19,
    "verse_key": "82:19",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 526,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167950,
        "resource_id": 31,
        "text": "Le jour où aucune âme ne pourra rien en faveur d’une autre âme. Et ce jour-là, le commandement sera à Allah."
      }
    ]
  }
]