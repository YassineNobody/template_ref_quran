[
  {
    "id": 4676,
    "verse_number": 1,
    "verse_key": "51:1",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 520,
    "juz_number": 26,
    "translations": [
      {
        "id": 165053,
        "resource_id": 31,
        "text": "Par les vents qui éparpillent !"
      }
    ]
  },
  {
    "id": 4677,
    "verse_number": 2,
    "verse_key": "51:2",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 520,
    "juz_number": 26,
    "translations": [
      {
        "id": 166433,
        "resource_id": 31,
        "text": "Par les porteurs de fardeaux !"
      }
    ]
  },
  {
    "id": 4678,
    "verse_number": 3,
    "verse_key": "51:3",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 520,
    "juz_number": 26,
    "translations": [
      {
        "id": 166401,
        "resource_id": 31,
        "text": "Par les glisseurs agiles !"
      }
    ]
  },
  {
    "id": 4679,
    "verse_number": 4,
    "verse_key": "51:4",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 520,
    "juz_number": 26,
    "translations": [
      {
        "id": 166403,
        "resource_id": 31,
        "text": "Par les distributeurs selon un commandement ! <sup foot_note=232768>1</sup>"
      }
    ]
  },
  {
    "id": 4680,
    "verse_number": 5,
    "verse_key": "51:5",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 520,
    "juz_number": 26,
    "translations": [
      {
        "id": 166417,
        "resource_id": 31,
        "text": "Ce qui vous est promis est certainement vrai."
      }
    ]
  },
  {
    "id": 4681,
    "verse_number": 6,
    "verse_key": "51:6",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 520,
    "juz_number": 26,
    "translations": [
      {
        "id": 166425,
        "resource_id": 31,
        "text": "Et la Rétribution arrivera inévitablement."
      }
    ]
  },
  {
    "id": 4682,
    "verse_number": 7,
    "verse_key": "51:7",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166427,
        "resource_id": 31,
        "text": "Par le ciel aux voies parfaitement tracées !"
      }
    ]
  },
  {
    "id": 4683,
    "verse_number": 8,
    "verse_key": "51:8",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166429,
        "resource_id": 31,
        "text": "Vous divergez sur ce que vous dites . <sup foot_note=232770>1</sup>"
      }
    ]
  },
  {
    "id": 4684,
    "verse_number": 9,
    "verse_key": "51:9",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166431,
        "resource_id": 31,
        "text": "Est détourné de lui  quiconque a été détourné de la foi. <sup foot_note=232771>1</sup>"
      }
    ]
  },
  {
    "id": 4685,
    "verse_number": 10,
    "verse_key": "51:10",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 168532,
        "resource_id": 31,
        "text": "Maudits soient les menteurs,"
      }
    ]
  },
  {
    "id": 4686,
    "verse_number": 11,
    "verse_key": "51:11",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166879,
        "resource_id": 31,
        "text": "qui sont plongés dans l’insouciance."
      }
    ]
  },
  {
    "id": 4687,
    "verse_number": 12,
    "verse_key": "51:12",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166362,
        "resource_id": 31,
        "text": "Ils demandent : \"A quand le jour de la Rétribution ?\""
      }
    ]
  },
  {
    "id": 4688,
    "verse_number": 13,
    "verse_key": "51:13",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166360,
        "resource_id": 31,
        "text": "Le jour où ils seront éprouvés au Feu :"
      }
    ]
  },
  {
    "id": 4689,
    "verse_number": 14,
    "verse_key": "51:14",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166369,
        "resource_id": 31,
        "text": "\"Goûtez à votre épreuve [punition] ; voici ce que vous cherchiez à hâter.\""
      }
    ]
  },
  {
    "id": 4690,
    "verse_number": 15,
    "verse_key": "51:15",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166364,
        "resource_id": 31,
        "text": "Les pieux seront dans des Jardins et [parmi] des sources,"
      }
    ]
  },
  {
    "id": 4691,
    "verse_number": 16,
    "verse_key": "51:16",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166371,
        "resource_id": 31,
        "text": "recevant ce que leur Seigneur leur aura donné. Car ils ont été auparavant de bienfaisants :"
      }
    ]
  },
  {
    "id": 4692,
    "verse_number": 17,
    "verse_key": "51:17",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166375,
        "resource_id": 31,
        "text": "ils dormaient peu, la nuit , <sup foot_note=232773>1</sup>"
      }
    ]
  },
  {
    "id": 4693,
    "verse_number": 18,
    "verse_key": "51:18",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166373,
        "resource_id": 31,
        "text": "et aux dernières heures de la nuit ils imploraient le pardon [d’Allah] ;"
      }
    ]
  },
  {
    "id": 4694,
    "verse_number": 19,
    "verse_key": "51:19",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166377,
        "resource_id": 31,
        "text": "et dans leurs biens, il y avait un droit au mendiant et au déshérité."
      }
    ]
  },
  {
    "id": 4695,
    "verse_number": 20,
    "verse_key": "51:20",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166379,
        "resource_id": 31,
        "text": "Il y a sur terre des preuves pour ceux qui croient avec certitude ;"
      }
    ]
  },
  {
    "id": 4696,
    "verse_number": 21,
    "verse_key": "51:21",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166387,
        "resource_id": 31,
        "text": "ainsi qu’en vous-mêmes. N’observez-vous donc pas ?"
      }
    ]
  },
  {
    "id": 4697,
    "verse_number": 22,
    "verse_key": "51:22",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166380,
        "resource_id": 31,
        "text": "Et il y dans le ciel votre subsistance et ce qui vous a été promis."
      }
    ]
  },
  {
    "id": 4698,
    "verse_number": 23,
    "verse_key": "51:23",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 457,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166381,
        "resource_id": 31,
        "text": "Par le Seigneur du ciel et de la terre ! Ceci est tout aussi vrai que le fait que vous parliez."
      }
    ]
  },
  {
    "id": 4699,
    "verse_number": 24,
    "verse_key": "51:24",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166382,
        "resource_id": 31,
        "text": "T’est-il parvenu le récit des visiteurs honorables d’Abraham ?"
      }
    ]
  },
  {
    "id": 4700,
    "verse_number": 25,
    "verse_key": "51:25",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166383,
        "resource_id": 31,
        "text": "Quand ils entrèrent chez lui et dirent : \"Paix!\", il [leur] dit : \"Paix, visiteurs inconnus.\""
      }
    ]
  },
  {
    "id": 4701,
    "verse_number": 26,
    "verse_key": "51:26",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166384,
        "resource_id": 31,
        "text": "Puis il alla discrètement à sa famille et apporta un veau gras."
      }
    ]
  },
  {
    "id": 4702,
    "verse_number": 27,
    "verse_key": "51:27",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166385,
        "resource_id": 31,
        "text": "Ensuite il l’approcha d’eux... \"Ne mangez-vous pas ?\" dit-il."
      }
    ]
  },
  {
    "id": 4703,
    "verse_number": 28,
    "verse_key": "51:28",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166386,
        "resource_id": 31,
        "text": "Il ressentit alors de la peur vis-à-vis d’eux. Ils dirent : \"N’aie pas peur.\" Et ils lui annoncèrent [la naissance] d’un garçon plein de savoir."
      }
    ]
  },
  {
    "id": 4704,
    "verse_number": 29,
    "verse_key": "51:29",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166389,
        "resource_id": 31,
        "text": "Alors sa femme s’avança en criant, se frappa le visage et dit : \"Une vieille femme stérile....\" <sup foot_note=232777>1</sup>"
      }
    ]
  },
  {
    "id": 4705,
    "verse_number": 30,
    "verse_key": "51:30",
    "hizb_number": 52,
    "rub_el_hizb_number": 208,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 521,
    "juz_number": 26,
    "translations": [
      {
        "id": 166399,
        "resource_id": 31,
        "text": "Ils dirent : \"Ainsi a dit ton Seigneur. C’est Lui vraiment le Sage, l’Omniscient.\""
      }
    ]
  },
  {
    "id": 4706,
    "verse_number": 31,
    "verse_key": "51:31",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166390,
        "resource_id": 31,
        "text": "Alors [Abraham] dit : \"Quelle est donc votre mission, ô envoyés ?\""
      }
    ]
  },
  {
    "id": 4707,
    "verse_number": 32,
    "verse_key": "51:32",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166391,
        "resource_id": 31,
        "text": "Ils dirent : \"Nous avons été envoyés vers des gens criminels,"
      }
    ]
  },
  {
    "id": 4708,
    "verse_number": 33,
    "verse_key": "51:33",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166392,
        "resource_id": 31,
        "text": "pour lancer sur eux des pierres de glaise,"
      }
    ]
  },
  {
    "id": 4709,
    "verse_number": 34,
    "verse_key": "51:34",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166393,
        "resource_id": 31,
        "text": "marquées auprès de ton Seigneur à l’intention des outranciers.\""
      }
    ]
  },
  {
    "id": 4710,
    "verse_number": 35,
    "verse_key": "51:35",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166394,
        "resource_id": 31,
        "text": "Nous en fîmes sortir alors ce qu’il y avait comme croyants,"
      }
    ]
  },
  {
    "id": 4711,
    "verse_number": 36,
    "verse_key": "51:36",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166395,
        "resource_id": 31,
        "text": "mais Nous n’y trouvâmes qu’une seule maison de gens soumis."
      }
    ]
  },
  {
    "id": 4712,
    "verse_number": 37,
    "verse_key": "51:37",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166396,
        "resource_id": 31,
        "text": "Et Nous y laissâmes un signe pour ceux qui redoutent le douloureux châtiment ;"
      }
    ]
  },
  {
    "id": 4713,
    "verse_number": 38,
    "verse_key": "51:38",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166397,
        "resource_id": 31,
        "text": "[Il y a même un signe] : en Moïse quand Nous l’envoyâmes, avec une preuve évidente, vers Pharaon."
      }
    ]
  },
  {
    "id": 4714,
    "verse_number": 39,
    "verse_key": "51:39",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166398,
        "resource_id": 31,
        "text": "Mais [celui-ci] : se détourna confiant en sa puissance, et dit : \"C’est un magicien ou un possédé !\""
      }
    ]
  },
  {
    "id": 4715,
    "verse_number": 40,
    "verse_key": "51:40",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166402,
        "resource_id": 31,
        "text": "Nous le saisîmes ainsi que ses troupes, puis les jetâmes dans les flots, pour son comportement blâmable."
      }
    ]
  },
  {
    "id": 4716,
    "verse_number": 41,
    "verse_key": "51:41",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166410,
        "resource_id": 31,
        "text": "De même pour les 'Âd, quand Nous envoyâmes contre eux le vent dévastateur."
      }
    ]
  },
  {
    "id": 4717,
    "verse_number": 42,
    "verse_key": "51:42",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166404,
        "resource_id": 31,
        "text": "n’épargnant rien sur son passage sans le réduire en poussière."
      }
    ]
  },
  {
    "id": 4718,
    "verse_number": 43,
    "verse_key": "51:43",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166405,
        "resource_id": 31,
        "text": "De même pour les Thamûd, quand il leur fut dit : \"Jouissez jusqu’à un certain temps !\""
      }
    ]
  },
  {
    "id": 4719,
    "verse_number": 44,
    "verse_key": "51:44",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166406,
        "resource_id": 31,
        "text": "Ils défièrent le commandement de leur Seigneur. La foudre les saisit alors qu’ils regardaient."
      }
    ]
  },
  {
    "id": 4720,
    "verse_number": 45,
    "verse_key": "51:45",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166407,
        "resource_id": 31,
        "text": "Ils ne purent ni se mettre debout ni être secourus."
      }
    ]
  },
  {
    "id": 4721,
    "verse_number": 46,
    "verse_key": "51:46",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 458,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166408,
        "resource_id": 31,
        "text": "De même, pour le peuple de Noé auparavant. Ils étaient des gens pervers."
      }
    ]
  },
  {
    "id": 4722,
    "verse_number": 47,
    "verse_key": "51:47",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166409,
        "resource_id": 31,
        "text": "Le ciel, Nous l’avons construit par Notre puissance : et Nous l’étendons [constamment] : dans l’immensité."
      }
    ]
  },
  {
    "id": 4723,
    "verse_number": 48,
    "verse_key": "51:48",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166414,
        "resource_id": 31,
        "text": "Et la terre, Nous l’avons étendue. Et de quelle excellente façon Nous l’avons nivelée !"
      }
    ]
  },
  {
    "id": 4724,
    "verse_number": 49,
    "verse_key": "51:49",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166412,
        "resource_id": 31,
        "text": "Et de toute chose Nous avons créé [deux éléments] : de couple. Peut-être vous rappellerez-vous ?"
      }
    ]
  },
  {
    "id": 4725,
    "verse_number": 50,
    "verse_key": "51:50",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166415,
        "resource_id": 31,
        "text": "\"Fuyez donc vers Allah. Moi, je suis pour vous de Sa part, un avertisseur explicite."
      }
    ]
  },
  {
    "id": 4726,
    "verse_number": 51,
    "verse_key": "51:51",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 522,
    "juz_number": 27,
    "translations": [
      {
        "id": 166416,
        "resource_id": 31,
        "text": "Ne placez pas avec Allah une autre divinité. Je suis pour vous de Sa part, un avertisseur explicite.\""
      }
    ]
  },
  {
    "id": 4727,
    "verse_number": 52,
    "verse_key": "51:52",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 523,
    "juz_number": 27,
    "translations": [
      {
        "id": 166418,
        "resource_id": 31,
        "text": "Ainsi aucun Messager n’est venu à leurs prédécesseurs sans qu’ils n’aient dit : \"C’est un magicien ou un possédé !\""
      }
    ]
  },
  {
    "id": 4728,
    "verse_number": 53,
    "verse_key": "51:53",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 523,
    "juz_number": 27,
    "translations": [
      {
        "id": 166419,
        "resource_id": 31,
        "text": "Est-ce qu’ils se sont transmis cette injonction ? Ils sont plutôt des gens transgresseurs."
      }
    ]
  },
  {
    "id": 4729,
    "verse_number": 54,
    "verse_key": "51:54",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 523,
    "juz_number": 27,
    "translations": [
      {
        "id": 166422,
        "resource_id": 31,
        "text": "Détourne-toi d’eux, tu ne seras pas blâmé [à leur sujet]."
      }
    ]
  },
  {
    "id": 4730,
    "verse_number": 55,
    "verse_key": "51:55",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 523,
    "juz_number": 27,
    "translations": [
      {
        "id": 166420,
        "resource_id": 31,
        "text": "Et rappelle ! Car le rappel profite aux croyants."
      }
    ]
  },
  {
    "id": 4731,
    "verse_number": 56,
    "verse_key": "51:56",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 523,
    "juz_number": 27,
    "translations": [
      {
        "id": 166331,
        "resource_id": 31,
        "text": "Je n’ai créé les djinns et les hommes que pour qu’ils M’adorent."
      }
    ]
  },
  {
    "id": 4732,
    "verse_number": 57,
    "verse_key": "51:57",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 523,
    "juz_number": 27,
    "translations": [
      {
        "id": 166365,
        "resource_id": 31,
        "text": "Je ne cherche pas d’eux une subsistance; et Je ne veux pas qu’ils me nourrissent."
      }
    ]
  },
  {
    "id": 4733,
    "verse_number": 58,
    "verse_key": "51:58",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 523,
    "juz_number": 27,
    "translations": [
      {
        "id": 166366,
        "resource_id": 31,
        "text": "En vérité, c’est Allah qui est le Grand Pourvoyeur, Le Détenteur de la force, l’Inébranlable."
      }
    ]
  },
  {
    "id": 4734,
    "verse_number": 59,
    "verse_key": "51:59",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 523,
    "juz_number": 27,
    "translations": [
      {
        "id": 166367,
        "resource_id": 31,
        "text": "Ceux qui ont été injustes auront une part [de tourments]: pareille à celle de leurs compagnons . [Qu'ils ne me demandent donc pas d'en hâter la venue !] <sup foot_note=232788>1</sup>"
      }
    ]
  },
  {
    "id": 4735,
    "verse_number": 60,
    "verse_key": "51:60",
    "hizb_number": 53,
    "rub_el_hizb_number": 209,
    "ruku_number": 459,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 523,
    "juz_number": 27,
    "translations": [
      {
        "id": 166423,
        "resource_id": 31,
        "text": "Malheur donc à ceux qui ont mécru à cause du jour dont ils sont menacés !"
      }
    ]
  }
]