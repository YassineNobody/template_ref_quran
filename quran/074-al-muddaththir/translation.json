[
  {
    "id": 5496,
    "verse_number": 1,
    "verse_key": "74:1",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167563,
        "resource_id": 31,
        "text": "Ô, toi (Mohammed) ! Le revêtu d’un manteau !"
      }
    ]
  },
  {
    "id": 5497,
    "verse_number": 2,
    "verse_key": "74:2",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 168218,
        "resource_id": 31,
        "text": "Lève-toi et avertis."
      }
    ]
  },
  {
    "id": 5498,
    "verse_number": 3,
    "verse_key": "74:3",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167614,
        "resource_id": 31,
        "text": "Et de ton Seigneur, célèbre la grandeur."
      }
    ]
  },
  {
    "id": 5499,
    "verse_number": 4,
    "verse_key": "74:4",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167604,
        "resource_id": 31,
        "text": "Et tes vêtements, purifie-les."
      }
    ]
  },
  {
    "id": 5500,
    "verse_number": 5,
    "verse_key": "74:5",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167612,
        "resource_id": 31,
        "text": "Et de tout péché, écarte-toi . <sup foot_note=233005>1</sup>"
      }
    ]
  },
  {
    "id": 5501,
    "verse_number": 6,
    "verse_key": "74:6",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167624,
        "resource_id": 31,
        "text": "Et ne donne pas dans le but de recevoir davantage . <sup foot_note=233007>1</sup>"
      }
    ]
  },
  {
    "id": 5502,
    "verse_number": 7,
    "verse_key": "74:7",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167616,
        "resource_id": 31,
        "text": "Et pour ton Seigneur, endure."
      }
    ]
  },
  {
    "id": 5503,
    "verse_number": 8,
    "verse_key": "74:8",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167618,
        "resource_id": 31,
        "text": "Quand on sonnera du Clairon,"
      }
    ]
  },
  {
    "id": 5504,
    "verse_number": 9,
    "verse_key": "74:9",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167620,
        "resource_id": 31,
        "text": "alors, ce jour-là sera un jour difficile,"
      }
    ]
  },
  {
    "id": 5505,
    "verse_number": 10,
    "verse_key": "74:10",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167564,
        "resource_id": 31,
        "text": "pas facile pour les mécréants."
      }
    ]
  },
  {
    "id": 5506,
    "verse_number": 11,
    "verse_key": "74:11",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167565,
        "resource_id": 31,
        "text": "Laisse-Moi avec celui que J’ai créé seul <sup foot_note=233009>1</sup>,"
      }
    ]
  },
  {
    "id": 5507,
    "verse_number": 12,
    "verse_key": "74:12",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167566,
        "resource_id": 31,
        "text": "et à qui J’ai donné des biens étendus,"
      }
    ]
  },
  {
    "id": 5508,
    "verse_number": 13,
    "verse_key": "74:13",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167567,
        "resource_id": 31,
        "text": "et des enfants qui lui tiennent toujours compagnie,"
      }
    ]
  },
  {
    "id": 5509,
    "verse_number": 14,
    "verse_key": "74:14",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167568,
        "resource_id": 31,
        "text": "pour qui aussi J’ai aplani toutes difficultés."
      }
    ]
  },
  {
    "id": 5510,
    "verse_number": 15,
    "verse_key": "74:15",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 159860,
        "resource_id": 31,
        "text": "Cependant, il convoite [de Moi] que Je lui donne davantage."
      }
    ]
  },
  {
    "id": 5511,
    "verse_number": 16,
    "verse_key": "74:16",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167446,
        "resource_id": 31,
        "text": "Pas du tout ! Car il reniait nos versets (le Coran) avec entêtement."
      }
    ]
  },
  {
    "id": 5512,
    "verse_number": 17,
    "verse_key": "74:17",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 575,
    "juz_number": 29,
    "translations": [
      {
        "id": 167525,
        "resource_id": 31,
        "text": "Je vais le contraindre à gravir une pente."
      }
    ]
  },
  {
    "id": 5513,
    "verse_number": 18,
    "verse_key": "74:18",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 160368,
        "resource_id": 31,
        "text": "Il a réfléchi. Et il a décidé."
      }
    ]
  },
  {
    "id": 5514,
    "verse_number": 19,
    "verse_key": "74:19",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 161239,
        "resource_id": 31,
        "text": "Qu’il périsse ! Comme il a décidé !"
      }
    ]
  },
  {
    "id": 5515,
    "verse_number": 20,
    "verse_key": "74:20",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167571,
        "resource_id": 31,
        "text": "Encore une fois, qu’il périsse; comme il a décidé !"
      }
    ]
  },
  {
    "id": 5516,
    "verse_number": 21,
    "verse_key": "74:21",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 168211,
        "resource_id": 31,
        "text": "Ensuite, il a regardé."
      }
    ]
  },
  {
    "id": 5517,
    "verse_number": 22,
    "verse_key": "74:22",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167581,
        "resource_id": 31,
        "text": "Et il s’est renfrogné et a durci son visage."
      }
    ]
  },
  {
    "id": 5518,
    "verse_number": 23,
    "verse_key": "74:23",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167572,
        "resource_id": 31,
        "text": "Ensuite il a tourné le dos et s’est enflé d’orgueil."
      }
    ]
  },
  {
    "id": 5519,
    "verse_number": 24,
    "verse_key": "74:24",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167573,
        "resource_id": 31,
        "text": "Puis il a dit : \"Ceci (le Coran) n’est que magie apprise."
      }
    ]
  },
  {
    "id": 5520,
    "verse_number": 25,
    "verse_key": "74:25",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167574,
        "resource_id": 31,
        "text": "ce n’est là que la parole d’un humain.\""
      }
    ]
  },
  {
    "id": 5521,
    "verse_number": 26,
    "verse_key": "74:26",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167575,
        "resource_id": 31,
        "text": "Je vais le brûler dans le Feu intense (Saqar)."
      }
    ]
  },
  {
    "id": 5522,
    "verse_number": 27,
    "verse_key": "74:27",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167576,
        "resource_id": 31,
        "text": "Et qui te dira ce qu’est Saqar ?"
      }
    ]
  },
  {
    "id": 5523,
    "verse_number": 28,
    "verse_key": "74:28",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167579,
        "resource_id": 31,
        "text": "Il ne laisse rien et n’épargne rien ;"
      }
    ]
  },
  {
    "id": 5524,
    "verse_number": 29,
    "verse_key": "74:29",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167577,
        "resource_id": 31,
        "text": "Il brûle la peau et la noircit."
      }
    ]
  },
  {
    "id": 5525,
    "verse_number": 30,
    "verse_key": "74:30",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167583,
        "resource_id": 31,
        "text": "Ils sont dix-neuf à y veiller <sup foot_note=233015>1</sup>."
      }
    ]
  },
  {
    "id": 5526,
    "verse_number": 31,
    "verse_key": "74:31",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 512,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167582,
        "resource_id": 31,
        "text": "Nous n’avons assigné comme gardiens du Feu que les Anges. Cependant, Nous n’en avons fixé le nombre que pour éprouver les mécréants, et aussi afin que ceux à qui le Livre a été apporté soient convaincus, et que croisse la foi de ceux qui croient, et que ceux à qui le Livre a été apporté et les croyants n’aient point de doute ; et pour que ceux qui ont au cœur quelque maladie ainsi que les mécréants disent : \"Qu’a donc voulu Allah par cette parabole ?\" C’est ainsi qu’Allah égare qui Il veut et guide qui Il veut. Nul ne connaît les armées de ton Seigneur, à part Lui. Et ce n’est là qu’un rappel pour les humains."
      }
    ]
  },
  {
    "id": 5527,
    "verse_number": 32,
    "verse_key": "74:32",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167592,
        "resource_id": 31,
        "text": "Non !... Par la lune !"
      }
    ]
  },
  {
    "id": 5528,
    "verse_number": 33,
    "verse_key": "74:33",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167585,
        "resource_id": 31,
        "text": "Et par la nuit quand elle se retire !"
      }
    ]
  },
  {
    "id": 5529,
    "verse_number": 34,
    "verse_key": "74:34",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167584,
        "resource_id": 31,
        "text": "Et par l’aurore quand elle se découvre !"
      }
    ]
  },
  {
    "id": 5530,
    "verse_number": 35,
    "verse_key": "74:35",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167586,
        "resource_id": 31,
        "text": "[Saqar] est l’un des plus grands [malheurs]"
      }
    ]
  },
  {
    "id": 5531,
    "verse_number": 36,
    "verse_key": "74:36",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167587,
        "resource_id": 31,
        "text": "un avertissement, pour les humains."
      }
    ]
  },
  {
    "id": 5532,
    "verse_number": 37,
    "verse_key": "74:37",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167588,
        "resource_id": 31,
        "text": "Pour qui d’entre vous, veut avancer ou reculer."
      }
    ]
  },
  {
    "id": 5533,
    "verse_number": 38,
    "verse_key": "74:38",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167589,
        "resource_id": 31,
        "text": "Toute âme est l’otage de ce qu’elle a acquis."
      }
    ]
  },
  {
    "id": 5534,
    "verse_number": 39,
    "verse_key": "74:39",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167590,
        "resource_id": 31,
        "text": "Sauf les gens de la droite (les élus) :"
      }
    ]
  },
  {
    "id": 5535,
    "verse_number": 40,
    "verse_key": "74:40",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167595,
        "resource_id": 31,
        "text": "dans des Jardins, ils s’interrogeront."
      }
    ]
  },
  {
    "id": 5536,
    "verse_number": 41,
    "verse_key": "74:41",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167593,
        "resource_id": 31,
        "text": "au sujet des criminels:"
      }
    ]
  },
  {
    "id": 5537,
    "verse_number": 42,
    "verse_key": "74:42",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167594,
        "resource_id": 31,
        "text": "\"Qu’est-ce qui vous a acheminés à Saqar ?\""
      }
    ]
  },
  {
    "id": 5538,
    "verse_number": 43,
    "verse_key": "74:43",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167596,
        "resource_id": 31,
        "text": "Ils diront : \"Nous n’étions pas de ceux qui faisaient la prière (As-Salât),"
      }
    ]
  },
  {
    "id": 5539,
    "verse_number": 44,
    "verse_key": "74:44",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167597,
        "resource_id": 31,
        "text": "et nous ne nourrissions pas le pauvre,"
      }
    ]
  },
  {
    "id": 5540,
    "verse_number": 45,
    "verse_key": "74:45",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167598,
        "resource_id": 31,
        "text": "et nous nous associions à ceux qui tenaient des conversations futiles,"
      }
    ]
  },
  {
    "id": 5541,
    "verse_number": 46,
    "verse_key": "74:46",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167599,
        "resource_id": 31,
        "text": "et nous traitions de mensonge le jour de la Rétribution,"
      }
    ]
  },
  {
    "id": 5542,
    "verse_number": 47,
    "verse_key": "74:47",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 576,
    "juz_number": 29,
    "translations": [
      {
        "id": 167600,
        "resource_id": 31,
        "text": "jusqu’à ce que nous vînt la vérité évidente [la mort].\""
      }
    ]
  },
  {
    "id": 5543,
    "verse_number": 48,
    "verse_key": "74:48",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167601,
        "resource_id": 31,
        "text": "Ne leur profitera point donc, l’intercession des intercesseurs."
      }
    ]
  },
  {
    "id": 5544,
    "verse_number": 49,
    "verse_key": "74:49",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167602,
        "resource_id": 31,
        "text": "Qu’ont-ils à se détourner du Rappel ?"
      }
    ]
  },
  {
    "id": 5545,
    "verse_number": 50,
    "verse_key": "74:50",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167605,
        "resource_id": 31,
        "text": "Ils sont comme des onagres épouvantés,"
      }
    ]
  },
  {
    "id": 5546,
    "verse_number": 51,
    "verse_key": "74:51",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167606,
        "resource_id": 31,
        "text": "s’enfuyant devant un lion."
      }
    ]
  },
  {
    "id": 5547,
    "verse_number": 52,
    "verse_key": "74:52",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167607,
        "resource_id": 31,
        "text": "Chacun d’eux voudrait plutôt qu’on lui apporte des feuilles tout étalées."
      }
    ]
  },
  {
    "id": 5548,
    "verse_number": 53,
    "verse_key": "74:53",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167608,
        "resource_id": 31,
        "text": "Ah ! Non ! C’est plutôt qu’ils ne craignent pas l’au-delà."
      }
    ]
  },
  {
    "id": 5549,
    "verse_number": 54,
    "verse_key": "74:54",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167609,
        "resource_id": 31,
        "text": "Ah ! Non ! Ceci est vraiment un Rappel."
      }
    ]
  },
  {
    "id": 5550,
    "verse_number": 55,
    "verse_key": "74:55",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167610,
        "resource_id": 31,
        "text": "Quiconque veut, qu’il se le rappelle."
      }
    ]
  },
  {
    "id": 5551,
    "verse_number": 56,
    "verse_key": "74:56",
    "hizb_number": 58,
    "rub_el_hizb_number": 230,
    "ruku_number": 513,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 577,
    "juz_number": 29,
    "translations": [
      {
        "id": 167611,
        "resource_id": 31,
        "text": "Mais ils ne se rappelleront que si Allah veut. C’est Lui qui est Le plus digne d’être craint; et c’est Lui qui détient le pardon."
      }
    ]
  }
]