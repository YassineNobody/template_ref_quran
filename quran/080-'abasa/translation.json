[
  {
    "id": 5759,
    "verse_number": 1,
    "verse_key": "80:1",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167908,
        "resource_id": 31,
        "text": "Il s’est renfrogné et il s’est détourné <sup foot_note=233060>1</sup>."
      }
    ]
  },
  {
    "id": 5760,
    "verse_number": 2,
    "verse_key": "80:2",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167877,
        "resource_id": 31,
        "text": "Parce que l’aveugle est venu à lui."
      }
    ]
  },
  {
    "id": 5761,
    "verse_number": 3,
    "verse_key": "80:3",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167887,
        "resource_id": 31,
        "text": "Qui te dit : peut-être [cherche]-t-il à se purifier."
      }
    ]
  },
  {
    "id": 5762,
    "verse_number": 4,
    "verse_key": "80:4",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167898,
        "resource_id": 31,
        "text": "Ou à se rappeler en sorte que le rappel lui profite."
      }
    ]
  },
  {
    "id": 5763,
    "verse_number": 5,
    "verse_key": "80:5",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167902,
        "resource_id": 31,
        "text": "Quant à celui qui se complait dans sa suffisance (pour sa richesse."
      }
    ]
  },
  {
    "id": 5764,
    "verse_number": 6,
    "verse_key": "80:6",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167903,
        "resource_id": 31,
        "text": "tu vas avec empressement à sa rencontre."
      }
    ]
  },
  {
    "id": 5765,
    "verse_number": 7,
    "verse_key": "80:7",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167904,
        "resource_id": 31,
        "text": "Or, que t’importe qu’il ne se purifie pas <sup foot_note=233061>1</sup>."
      }
    ]
  },
  {
    "id": 5766,
    "verse_number": 8,
    "verse_key": "80:8",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167905,
        "resource_id": 31,
        "text": "Et quant à celui qui vient à toi avec empressement."
      }
    ]
  },
  {
    "id": 5767,
    "verse_number": 9,
    "verse_key": "80:9",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167906,
        "resource_id": 31,
        "text": "tout en ayant la crainte."
      }
    ]
  },
  {
    "id": 5768,
    "verse_number": 10,
    "verse_key": "80:10",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167873,
        "resource_id": 31,
        "text": "tu ne t’en soucies pas."
      }
    ]
  },
  {
    "id": 5769,
    "verse_number": 11,
    "verse_key": "80:11",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167874,
        "resource_id": 31,
        "text": "N’agis plus ainsi ! Vraiment ceci est un rappel,"
      }
    ]
  },
  {
    "id": 5770,
    "verse_number": 12,
    "verse_key": "80:12",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167875,
        "resource_id": 31,
        "text": "quiconque veut, donc, s’en rappelle,"
      }
    ]
  },
  {
    "id": 5771,
    "verse_number": 13,
    "verse_key": "80:13",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 163249,
        "resource_id": 31,
        "text": "consigné dans des feuilles honorées,"
      }
    ]
  },
  {
    "id": 5772,
    "verse_number": 14,
    "verse_key": "80:14",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 163323,
        "resource_id": 31,
        "text": "élevées, purifiées,"
      }
    ]
  },
  {
    "id": 5773,
    "verse_number": 15,
    "verse_key": "80:15",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 164273,
        "resource_id": 31,
        "text": "entre les mains d’ambassadeur <sup foot_note=233063>1</sup>,"
      }
    ]
  },
  {
    "id": 5774,
    "verse_number": 16,
    "verse_key": "80:16",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 164585,
        "resource_id": 31,
        "text": "nobles, obéissants."
      }
    ]
  },
  {
    "id": 5775,
    "verse_number": 17,
    "verse_key": "80:17",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 164620,
        "resource_id": 31,
        "text": "Que périsse l’homme ! Qu’il est ingrat."
      }
    ]
  },
  {
    "id": 5776,
    "verse_number": 18,
    "verse_key": "80:18",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 165055,
        "resource_id": 31,
        "text": "De quoi [Dieu] l’a-t-Il créé."
      }
    ]
  },
  {
    "id": 5777,
    "verse_number": 19,
    "verse_key": "80:19",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167876,
        "resource_id": 31,
        "text": "D’une goutte de sperme, Il le crée et détermine (son destin)"
      }
    ]
  },
  {
    "id": 5778,
    "verse_number": 20,
    "verse_key": "80:20",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167878,
        "resource_id": 31,
        "text": "puis Il lui facilite le chemin,"
      }
    ]
  },
  {
    "id": 5779,
    "verse_number": 21,
    "verse_key": "80:21",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167879,
        "resource_id": 31,
        "text": "puis Il lui donne la mort et le met au tombeau,"
      }
    ]
  },
  {
    "id": 5780,
    "verse_number": 22,
    "verse_key": "80:22",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167880,
        "resource_id": 31,
        "text": "puis Il le ressuscitera quand Il voudra."
      }
    ]
  },
  {
    "id": 5781,
    "verse_number": 23,
    "verse_key": "80:23",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167881,
        "resource_id": 31,
        "text": "Eh bien non! [L’homme] n’accomplit pas ce qu’Il lui commande."
      }
    ]
  },
  {
    "id": 5782,
    "verse_number": 24,
    "verse_key": "80:24",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167882,
        "resource_id": 31,
        "text": "Que l’homme considère donc sa nourriture."
      }
    ]
  },
  {
    "id": 5783,
    "verse_number": 25,
    "verse_key": "80:25",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167883,
        "resource_id": 31,
        "text": "C’est Nous qui versons l’eau abondante."
      }
    ]
  },
  {
    "id": 5784,
    "verse_number": 26,
    "verse_key": "80:26",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167884,
        "resource_id": 31,
        "text": "Puis Nous fendons la terre par fissure."
      }
    ]
  },
  {
    "id": 5785,
    "verse_number": 27,
    "verse_key": "80:27",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167885,
        "resource_id": 31,
        "text": "Et y faisons pousser grains,"
      }
    ]
  },
  {
    "id": 5786,
    "verse_number": 28,
    "verse_key": "80:28",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167886,
        "resource_id": 31,
        "text": "vignobles et légumes,"
      }
    ]
  },
  {
    "id": 5787,
    "verse_number": 29,
    "verse_key": "80:29",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 165065,
        "resource_id": 31,
        "text": "oliviers et palmiers,"
      }
    ]
  },
  {
    "id": 5788,
    "verse_number": 30,
    "verse_key": "80:30",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167888,
        "resource_id": 31,
        "text": "jardins touffus,"
      }
    ]
  },
  {
    "id": 5789,
    "verse_number": 31,
    "verse_key": "80:31",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167889,
        "resource_id": 31,
        "text": "fruits et herbages,"
      }
    ]
  },
  {
    "id": 5790,
    "verse_number": 32,
    "verse_key": "80:32",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167890,
        "resource_id": 31,
        "text": "pour votre jouissance vous et vos bestiaux."
      }
    ]
  },
  {
    "id": 5791,
    "verse_number": 33,
    "verse_key": "80:33",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167891,
        "resource_id": 31,
        "text": "Puis quand viendra le Fracas."
      }
    ]
  },
  {
    "id": 5792,
    "verse_number": 34,
    "verse_key": "80:34",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167892,
        "resource_id": 31,
        "text": "Le jour où l’homme s’enfuira de son frère."
      }
    ]
  },
  {
    "id": 5793,
    "verse_number": 35,
    "verse_key": "80:35",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167893,
        "resource_id": 31,
        "text": "De sa mère, de son père."
      }
    ]
  },
  {
    "id": 5794,
    "verse_number": 36,
    "verse_key": "80:36",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167894,
        "resource_id": 31,
        "text": "De sa compagne et de ses enfants."
      }
    ]
  },
  {
    "id": 5795,
    "verse_number": 37,
    "verse_key": "80:37",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167895,
        "resource_id": 31,
        "text": "Car chacun d’eux, ce jour-là, aura son propre cas pour l’occuper."
      }
    ]
  },
  {
    "id": 5796,
    "verse_number": 38,
    "verse_key": "80:38",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167896,
        "resource_id": 31,
        "text": "Ce jour-là, il y aura des visages rayonnants,"
      }
    ]
  },
  {
    "id": 5797,
    "verse_number": 39,
    "verse_key": "80:39",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167897,
        "resource_id": 31,
        "text": "riants et réjouis."
      }
    ]
  },
  {
    "id": 5798,
    "verse_number": 40,
    "verse_key": "80:40",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167899,
        "resource_id": 31,
        "text": "De même qu’il y aura, ce jour-là, des visages couverts de poussière,"
      }
    ]
  },
  {
    "id": 5799,
    "verse_number": 41,
    "verse_key": "80:41",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167900,
        "resource_id": 31,
        "text": "recouverts de ténèbres."
      }
    ]
  },
  {
    "id": 5800,
    "verse_number": 42,
    "verse_key": "80:42",
    "hizb_number": 59,
    "rub_el_hizb_number": 234,
    "ruku_number": 524,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 585,
    "juz_number": 30,
    "translations": [
      {
        "id": 167901,
        "resource_id": 31,
        "text": "Voilà les infidèles, les libertins."
      }
    ]
  }
]