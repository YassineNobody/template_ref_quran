[
  {
    "id": 4415,
    "verse_number": 1,
    "verse_key": "44:1",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 164984,
        "resource_id": 31,
        "text": "Hâ, Mîm . <sup foot_note=232664>1</sup>"
      }
    ]
  },
  {
    "id": 4416,
    "verse_number": 2,
    "verse_key": "44:2",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166048,
        "resource_id": 31,
        "text": "Par le Livre (le Coran) explicite."
      }
    ]
  },
  {
    "id": 4417,
    "verse_number": 3,
    "verse_key": "44:3",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166056,
        "resource_id": 31,
        "text": "Nous l’avons fait descendre en une nuit bénie, Nous sommes en vérité Celui qui avertit,"
      }
    ]
  },
  {
    "id": 4418,
    "verse_number": 4,
    "verse_key": "44:4",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166062,
        "resource_id": 31,
        "text": "durant laquelle est décidé tout ordre sage,"
      }
    ]
  },
  {
    "id": 4419,
    "verse_number": 5,
    "verse_key": "44:5",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166071,
        "resource_id": 31,
        "text": "c’est là un commandement venant de Nous. C’est Nous qui envoyons [les Messagers],"
      }
    ]
  },
  {
    "id": 4420,
    "verse_number": 6,
    "verse_key": "44:6",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166087,
        "resource_id": 31,
        "text": "à titre de miséricordieux de la part de ton Seigneur, car c’est Lui l’Audient, l’Omniscient,"
      }
    ]
  },
  {
    "id": 4421,
    "verse_number": 7,
    "verse_key": "44:7",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166088,
        "resource_id": 31,
        "text": "Seigneur des cieux et de la terre et de ce qui est entre eux, si seulement vous pouviez en avoir la conviction."
      }
    ]
  },
  {
    "id": 4422,
    "verse_number": 8,
    "verse_key": "44:8",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166090,
        "resource_id": 31,
        "text": "Point de divinité à part Lui. Il donne la vie et donne la mort, et Il est votre Seigneur et le Seigneur de vos premiers ancêtres."
      }
    ]
  },
  {
    "id": 4423,
    "verse_number": 9,
    "verse_key": "44:9",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 164986,
        "resource_id": 31,
        "text": "Mais ces gens-là, dans le doute, s’amusent."
      }
    ]
  },
  {
    "id": 4424,
    "verse_number": 10,
    "verse_key": "44:10",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166026,
        "resource_id": 31,
        "text": "Et bien, attends le jour où le ciel apportera une fumée visible."
      }
    ]
  },
  {
    "id": 4425,
    "verse_number": 11,
    "verse_key": "44:11",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166033,
        "resource_id": 31,
        "text": "qui couvrira les gens. Ce sera un châtiment douloureux."
      }
    ]
  },
  {
    "id": 4426,
    "verse_number": 12,
    "verse_key": "44:12",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166027,
        "resource_id": 31,
        "text": "\"Seigneur, éloigne de nous le châtiment. Car [à présent] nous croyons.\""
      }
    ]
  },
  {
    "id": 4427,
    "verse_number": 13,
    "verse_key": "44:13",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166028,
        "resource_id": 31,
        "text": "D’où leur vient cette prise de conscience  alors qu’un Messager explicite leur est déjà venu. <sup foot_note=232666>1</sup>"
      }
    ]
  },
  {
    "id": 4428,
    "verse_number": 14,
    "verse_key": "44:14",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166029,
        "resource_id": 31,
        "text": "Puis ils s’en détournèrent en disant: \"C’est un homme instruit [par d’autres], un possédé.\""
      }
    ]
  },
  {
    "id": 4429,
    "verse_number": 15,
    "verse_key": "44:15",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166030,
        "resource_id": 31,
        "text": "Nous dissiperons le châtiment pour peu de temps; car vous récidiverez."
      }
    ]
  },
  {
    "id": 4430,
    "verse_number": 16,
    "verse_key": "44:16",
    "hizb_number": 50,
    "rub_el_hizb_number": 198,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166031,
        "resource_id": 31,
        "text": "Le jour où Nous userons de la plus grande violence et Nous Nous vengerons."
      }
    ]
  },
  {
    "id": 4431,
    "verse_number": 17,
    "verse_key": "44:17",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166032,
        "resource_id": 31,
        "text": "Et avant eux, Nous avons déjà éprouvé le peuple de Pharaon, quand un noble Messager leur était venu , <sup foot_note=232667>1</sup>"
      }
    ]
  },
  {
    "id": 4432,
    "verse_number": 18,
    "verse_key": "44:18",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 496,
    "juz_number": 25,
    "translations": [
      {
        "id": 166036,
        "resource_id": 31,
        "text": "[leur disant] : \"Livrez-moi les serviteurs d’Allah ! Je suis pour vous un Messager digne de confiance."
      }
    ]
  },
  {
    "id": 4433,
    "verse_number": 19,
    "verse_key": "44:19",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166034,
        "resource_id": 31,
        "text": "Ne vous montrez pas hautains vis-à-vis d’Allah, car je vous apporte une preuve évidente."
      }
    ]
  },
  {
    "id": 4434,
    "verse_number": 20,
    "verse_key": "44:20",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166037,
        "resource_id": 31,
        "text": "Et je cherche protection auprès de mon Seigneur et votre Seigneur, pour que vous ne me lapidiez pas."
      }
    ]
  },
  {
    "id": 4435,
    "verse_number": 21,
    "verse_key": "44:21",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166038,
        "resource_id": 31,
        "text": "Si vous ne voulez pas croire en moi, éloignez-vous de moi.\""
      }
    ]
  },
  {
    "id": 4436,
    "verse_number": 22,
    "verse_key": "44:22",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166039,
        "resource_id": 31,
        "text": "Il invoqua alors son Seigneur : \"Ce sont des gens criminels.\""
      }
    ]
  },
  {
    "id": 4437,
    "verse_number": 23,
    "verse_key": "44:23",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166040,
        "resource_id": 31,
        "text": "\"Voyage de nuit avec Mes serviteurs; vous serez poursuivis . <sup foot_note=232670>1</sup>"
      }
    ]
  },
  {
    "id": 4438,
    "verse_number": 24,
    "verse_key": "44:24",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166041,
        "resource_id": 31,
        "text": "Laisse la mer calme ; [telle que tu l’as franchie] ce sont, des armées [voués] à la noyade.\""
      }
    ]
  },
  {
    "id": 4439,
    "verse_number": 25,
    "verse_key": "44:25",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166042,
        "resource_id": 31,
        "text": "Que de jardins et de sources ils laissèrent [derrière eux]"
      }
    ]
  },
  {
    "id": 4440,
    "verse_number": 26,
    "verse_key": "44:26",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166043,
        "resource_id": 31,
        "text": "que de champs et de superbes résidences,"
      }
    ]
  },
  {
    "id": 4441,
    "verse_number": 27,
    "verse_key": "44:27",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166044,
        "resource_id": 31,
        "text": "que de délices au sein desquels ils réjouissaient."
      }
    ]
  },
  {
    "id": 4442,
    "verse_number": 28,
    "verse_key": "44:28",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166045,
        "resource_id": 31,
        "text": "Il en fut ainsi et Nous fîmes qu’un autre peuple en hérita."
      }
    ]
  },
  {
    "id": 4443,
    "verse_number": 29,
    "verse_key": "44:29",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 433,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166046,
        "resource_id": 31,
        "text": "Ni le ciel ni la terre ne les pleurèrent et ils n’eurent aucun délai."
      }
    ]
  },
  {
    "id": 4444,
    "verse_number": 30,
    "verse_key": "44:30",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166049,
        "resource_id": 31,
        "text": "Et certes, Nous sauvâmes les Enfants d’Israël du châtiment avilissant."
      }
    ]
  },
  {
    "id": 4445,
    "verse_number": 31,
    "verse_key": "44:31",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 168582,
        "resource_id": 31,
        "text": "de Pharaon qui était hautain et outrancier."
      }
    ]
  },
  {
    "id": 4446,
    "verse_number": 32,
    "verse_key": "44:32",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166050,
        "resource_id": 31,
        "text": "A bon escient Nous les choisîmes parmi tous les peuples de l’univers , <sup foot_note=232673>1</sup>"
      }
    ]
  },
  {
    "id": 4447,
    "verse_number": 33,
    "verse_key": "44:33",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166051,
        "resource_id": 31,
        "text": "et leur apportâmes des miracles de quoi les mettre manifestement à l’épreuve."
      }
    ]
  },
  {
    "id": 4448,
    "verse_number": 34,
    "verse_key": "44:34",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166052,
        "resource_id": 31,
        "text": "Ceux-là (les Mecquois) disent :"
      }
    ]
  },
  {
    "id": 4449,
    "verse_number": 35,
    "verse_key": "44:35",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166053,
        "resource_id": 31,
        "text": "\"Il n’y a pour nous qu’une mort, la première. Et nous ne serons pas ressuscités."
      }
    ]
  },
  {
    "id": 4450,
    "verse_number": 36,
    "verse_key": "44:36",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166054,
        "resource_id": 31,
        "text": "Faites donc revenir nos ancêtres, si vous êtes véridiques.\""
      }
    ]
  },
  {
    "id": 4451,
    "verse_number": 37,
    "verse_key": "44:37",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166055,
        "resource_id": 31,
        "text": "Sont-ils les meilleurs ou le peuple de Tubbaa  et ceux qui les ont précédés ? Nous les avons périr parce que vraiment ils étaient criminels. <sup foot_note=232675>1</sup>"
      }
    ]
  },
  {
    "id": 4452,
    "verse_number": 38,
    "verse_key": "44:38",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166059,
        "resource_id": 31,
        "text": "Ce n’est pas par divertissement que Nous avons créé les cieux et la Terre et ce qui est entre eux."
      }
    ]
  },
  {
    "id": 4453,
    "verse_number": 39,
    "verse_key": "44:39",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 497,
    "juz_number": 25,
    "translations": [
      {
        "id": 166057,
        "resource_id": 31,
        "text": "Nous ne les avons créés qu’en toute vérité. Mais la plupart d’entre eux ne savent pas."
      }
    ]
  },
  {
    "id": 4454,
    "verse_number": 40,
    "verse_key": "44:40",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166060,
        "resource_id": 31,
        "text": "En vérité, le Jour de la Décision  sera leur rendez- vous à tous, <sup foot_note=232676>1</sup>"
      }
    ]
  },
  {
    "id": 4455,
    "verse_number": 41,
    "verse_key": "44:41",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166061,
        "resource_id": 31,
        "text": "Le jour où un allié ne sera d’aucune utilité à un [autre] allié ; et ils ne seront point secourus non plus,"
      }
    ]
  },
  {
    "id": 4456,
    "verse_number": 42,
    "verse_key": "44:42",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 434,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166079,
        "resource_id": 31,
        "text": "sauf celui qui Allah fera miséricorde. Car c’est Lui, le Puissant, le Très Miséricordieux."
      }
    ]
  },
  {
    "id": 4457,
    "verse_number": 43,
    "verse_key": "44:43",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166063,
        "resource_id": 31,
        "text": "Certes l’arbre de Zaqûm."
      }
    ]
  },
  {
    "id": 4458,
    "verse_number": 44,
    "verse_key": "44:44",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166065,
        "resource_id": 31,
        "text": "sera la nourriture du grand pécheur."
      }
    ]
  },
  {
    "id": 4459,
    "verse_number": 45,
    "verse_key": "44:45",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166064,
        "resource_id": 31,
        "text": "Comme du métal en fusion ; il bouillonnera dans les ventres."
      }
    ]
  },
  {
    "id": 4460,
    "verse_number": 46,
    "verse_key": "44:46",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166066,
        "resource_id": 31,
        "text": "comme le bouillonnement de l’eau surchauffée."
      }
    ]
  },
  {
    "id": 4461,
    "verse_number": 47,
    "verse_key": "44:47",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166067,
        "resource_id": 31,
        "text": "Qu’on le saisisse et qu’on l’emporte en plein dans la fournaise ;"
      }
    ]
  },
  {
    "id": 4462,
    "verse_number": 48,
    "verse_key": "44:48",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166068,
        "resource_id": 31,
        "text": "qu’on verse ensuite sur sa tête de l’eau bouillante comme châtiment."
      }
    ]
  },
  {
    "id": 4463,
    "verse_number": 49,
    "verse_key": "44:49",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166069,
        "resource_id": 31,
        "text": "Goûte! Toi [qui prétendait être] le puissant, le noble."
      }
    ]
  },
  {
    "id": 4464,
    "verse_number": 50,
    "verse_key": "44:50",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166072,
        "resource_id": 31,
        "text": "Voilà ce dont vous doutiez."
      }
    ]
  },
  {
    "id": 4465,
    "verse_number": 51,
    "verse_key": "44:51",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166073,
        "resource_id": 31,
        "text": "Les pieux seront dans une demeure sûre,"
      }
    ]
  },
  {
    "id": 4466,
    "verse_number": 52,
    "verse_key": "44:52",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166074,
        "resource_id": 31,
        "text": "parmi les jardins et des sources,"
      }
    ]
  },
  {
    "id": 4467,
    "verse_number": 53,
    "verse_key": "44:53",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166075,
        "resource_id": 31,
        "text": "Ils porteront des vêtements de satin et de brocart et seront placés face à face."
      }
    ]
  },
  {
    "id": 4468,
    "verse_number": 54,
    "verse_key": "44:54",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166076,
        "resource_id": 31,
        "text": "C’est ainsi ! Et Nous leur donnerons pour épouses des houris aux grands yeux."
      }
    ]
  },
  {
    "id": 4469,
    "verse_number": 55,
    "verse_key": "44:55",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166077,
        "resource_id": 31,
        "text": "Ils y demanderont en toute quiétude toutes sortes de fruits."
      }
    ]
  },
  {
    "id": 4470,
    "verse_number": 56,
    "verse_key": "44:56",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166078,
        "resource_id": 31,
        "text": "Ils n’y goûteront pas à la mort sauf leur mort première. Et [Allah] les protégera du châtiment de la Fournaise,"
      }
    ]
  },
  {
    "id": 4471,
    "verse_number": 57,
    "verse_key": "44:57",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166085,
        "resource_id": 31,
        "text": "c’est là une grâce de ton Seigneur. Et c’est là l’énorme succès."
      }
    ]
  },
  {
    "id": 4472,
    "verse_number": 58,
    "verse_key": "44:58",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166080,
        "resource_id": 31,
        "text": "Nous ne l’avons facilité  dans ta langue, qu’afin qu’ils se rappellent ! <sup foot_note=232681>1</sup>"
      }
    ]
  },
  {
    "id": 4473,
    "verse_number": 59,
    "verse_key": "44:59",
    "hizb_number": 50,
    "rub_el_hizb_number": 199,
    "ruku_number": 435,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 498,
    "juz_number": 25,
    "translations": [
      {
        "id": 166081,
        "resource_id": 31,
        "text": "Attends donc. Eux aussi attendent."
      }
    ]
  }
]