[
  {
    "id": 5324,
    "verse_number": 1,
    "verse_key": "69:1",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 566,
    "juz_number": 29,
    "translations": [
      {
        "id": 167256,
        "resource_id": 31,
        "text": "L’inévitable [l’Heure qui montre la vérité]."
      }
    ]
  },
  {
    "id": 5325,
    "verse_number": 2,
    "verse_key": "69:2",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 566,
    "juz_number": 29,
    "translations": [
      {
        "id": 167304,
        "resource_id": 31,
        "text": "Qu’est-ce que l’inévitable ?"
      }
    ]
  },
  {
    "id": 5326,
    "verse_number": 3,
    "verse_key": "69:3",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 566,
    "juz_number": 29,
    "translations": [
      {
        "id": 167271,
        "resource_id": 31,
        "text": "Et qui te dira ce que c’est que l’inévitable ?"
      }
    ]
  },
  {
    "id": 5327,
    "verse_number": 4,
    "verse_key": "69:4",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 566,
    "juz_number": 29,
    "translations": [
      {
        "id": 167283,
        "resource_id": 31,
        "text": "Les Thamûd et les 'Âd avaient traité de mensonge le cataclysme."
      }
    ]
  },
  {
    "id": 5328,
    "verse_number": 5,
    "verse_key": "69:5",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 566,
    "juz_number": 29,
    "translations": [
      {
        "id": 167297,
        "resource_id": 31,
        "text": "Quant aux Thamûd, ils furent détruits par le [bruit] excessivement fort."
      }
    ]
  },
  {
    "id": 5329,
    "verse_number": 6,
    "verse_key": "69:6",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 566,
    "juz_number": 29,
    "translations": [
      {
        "id": 167299,
        "resource_id": 31,
        "text": "Et quant aux 'Âd, ils furent détruits par un vent mugissant et furieux."
      }
    ]
  },
  {
    "id": 5330,
    "verse_number": 7,
    "verse_key": "69:7",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 566,
    "juz_number": 29,
    "translations": [
      {
        "id": 167300,
        "resource_id": 31,
        "text": "qu’ [Allah] déchaîna contre eux pendant sept nuits et huit jours consécutifs; tu voyais alors les gens renversés par terre comme des souches de palmiers évidées."
      }
    ]
  },
  {
    "id": 5331,
    "verse_number": 8,
    "verse_key": "69:8",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 566,
    "juz_number": 29,
    "translations": [
      {
        "id": 167302,
        "resource_id": 31,
        "text": "En vois-tu le moindre vestige ?"
      }
    ]
  },
  {
    "id": 5332,
    "verse_number": 9,
    "verse_key": "69:9",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167308,
        "resource_id": 31,
        "text": "Pharaon et ceux qui vécurent avant lui ainsi que les Villes renversées commirent des fautes . <sup foot_note=232959>1</sup>"
      }
    ]
  },
  {
    "id": 5333,
    "verse_number": 10,
    "verse_key": "69:10",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167254,
        "resource_id": 31,
        "text": "Ils désobéirent au Messager de leur Seigneur. Celui-ci donc, les saisit d’une façon irrésistible."
      }
    ]
  },
  {
    "id": 5334,
    "verse_number": 11,
    "verse_key": "69:11",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167253,
        "resource_id": 31,
        "text": "C’est Nous qui, quand l’eau déborda , vous avons chargés sur l’Arche. <sup foot_note=232961>1</sup>"
      }
    ]
  },
  {
    "id": 5335,
    "verse_number": 12,
    "verse_key": "69:12",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 166960,
        "resource_id": 31,
        "text": "afin d’en faire pour vous un rappel que toute oreille fidèle conserve."
      }
    ]
  },
  {
    "id": 5336,
    "verse_number": 13,
    "verse_key": "69:13",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 166991,
        "resource_id": 31,
        "text": "Puis, quand d’un seul souffle, on soufflera dans la Trompe,"
      }
    ]
  },
  {
    "id": 5337,
    "verse_number": 14,
    "verse_key": "69:14",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167056,
        "resource_id": 31,
        "text": "et que la terre et les montagnes seront soulevées puis tassées d’un seul coup ;"
      }
    ]
  },
  {
    "id": 5338,
    "verse_number": 15,
    "verse_key": "69:15",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167121,
        "resource_id": 31,
        "text": "Ce jour-là alors, l’Evénement se produira,"
      }
    ]
  },
  {
    "id": 5339,
    "verse_number": 16,
    "verse_key": "69:16",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167122,
        "resource_id": 31,
        "text": "et le ciel se fendra et sera fragile, ce jour-là."
      }
    ]
  },
  {
    "id": 5340,
    "verse_number": 17,
    "verse_key": "69:17",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167255,
        "resource_id": 31,
        "text": "Et sur ses côtés [se tiendront] les Anges, tandis que huit, ce jour-là, porteront au-dessus d’eux le Trône de ton Seigneur."
      }
    ]
  },
  {
    "id": 5341,
    "verse_number": 18,
    "verse_key": "69:18",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167259,
        "resource_id": 31,
        "text": "Ce jour-là vous serez exposés; et rien de vous ne sera caché."
      }
    ]
  },
  {
    "id": 5342,
    "verse_number": 19,
    "verse_key": "69:19",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167257,
        "resource_id": 31,
        "text": "Quant à celui à qui on aura remis le Livre en sa main droite, il dira : \"Tenez ! Lisez mon livre."
      }
    ]
  },
  {
    "id": 5343,
    "verse_number": 20,
    "verse_key": "69:20",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167265,
        "resource_id": 31,
        "text": "J’étais sûr d’y trouver mon compte.\""
      }
    ]
  },
  {
    "id": 5344,
    "verse_number": 21,
    "verse_key": "69:21",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167260,
        "resource_id": 31,
        "text": "Il jouira d’une vie agréable:"
      }
    ]
  },
  {
    "id": 5345,
    "verse_number": 22,
    "verse_key": "69:22",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167262,
        "resource_id": 31,
        "text": "dans un Jardin haut placé."
      }
    ]
  },
  {
    "id": 5346,
    "verse_number": 23,
    "verse_key": "69:23",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167261,
        "resource_id": 31,
        "text": "dont les fruits sont à portées de la main."
      }
    ]
  },
  {
    "id": 5347,
    "verse_number": 24,
    "verse_key": "69:24",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167263,
        "resource_id": 31,
        "text": "\"Mangez et buvez agréablement pour ce que vous avez avancé dans les jours passés.\""
      }
    ]
  },
  {
    "id": 5348,
    "verse_number": 25,
    "verse_key": "69:25",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167264,
        "resource_id": 31,
        "text": "Quant à celui à qui on aura remis le Livre en sa main gauche, il dira : \"Hélas pour moi ! J’aurai souhaité qu’on ne m’ait pas remis mon livre,"
      }
    ]
  },
  {
    "id": 5349,
    "verse_number": 26,
    "verse_key": "69:26",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167294,
        "resource_id": 31,
        "text": "et ne pas avoir connu mon compte..."
      }
    ]
  },
  {
    "id": 5350,
    "verse_number": 27,
    "verse_key": "69:27",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167266,
        "resource_id": 31,
        "text": "Hélas, comme j’aurais souhaité que [ma première mort] fût la définitive."
      }
    ]
  },
  {
    "id": 5351,
    "verse_number": 28,
    "verse_key": "69:28",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167269,
        "resource_id": 31,
        "text": "Ma fortune ne m’a servi à rien."
      }
    ]
  },
  {
    "id": 5352,
    "verse_number": 29,
    "verse_key": "69:29",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167267,
        "resource_id": 31,
        "text": "Mon autorité est anéantie et m’a quitté !\""
      }
    ]
  },
  {
    "id": 5353,
    "verse_number": 30,
    "verse_key": "69:30",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167270,
        "resource_id": 31,
        "text": "\"Saisissez-le ! Puis, mettez-lui un carcan ;"
      }
    ]
  },
  {
    "id": 5354,
    "verse_number": 31,
    "verse_key": "69:31",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167272,
        "resource_id": 31,
        "text": "ensuite, brûlez-le dans la Fournaise ;"
      }
    ]
  },
  {
    "id": 5355,
    "verse_number": 32,
    "verse_key": "69:32",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167273,
        "resource_id": 31,
        "text": "puis, liez-le avec une chaîne de soixante-dix coudées,"
      }
    ]
  },
  {
    "id": 5356,
    "verse_number": 33,
    "verse_key": "69:33",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167274,
        "resource_id": 31,
        "text": "car il ne croyait pas en Allah, le Très Grand."
      }
    ]
  },
  {
    "id": 5357,
    "verse_number": 34,
    "verse_key": "69:34",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 567,
    "juz_number": 29,
    "translations": [
      {
        "id": 167275,
        "resource_id": 31,
        "text": "Et n’incitait pas à nourrir le pauvre."
      }
    ]
  },
  {
    "id": 5358,
    "verse_number": 35,
    "verse_key": "69:35",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167276,
        "resource_id": 31,
        "text": "Il n’a pour lui ici, aujourd’hui, point d’ami chaleureux [pour le protéger],"
      }
    ]
  },
  {
    "id": 5359,
    "verse_number": 36,
    "verse_key": "69:36",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167281,
        "resource_id": 31,
        "text": "ni d’autre nourriture que du pus,"
      }
    ]
  },
  {
    "id": 5360,
    "verse_number": 37,
    "verse_key": "69:37",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 502,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167277,
        "resource_id": 31,
        "text": "que seuls les fautifs mangeront.\""
      }
    ]
  },
  {
    "id": 5361,
    "verse_number": 38,
    "verse_key": "69:38",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167278,
        "resource_id": 31,
        "text": "Mais non... Je jure par ce que vous voyez,"
      }
    ]
  },
  {
    "id": 5362,
    "verse_number": 39,
    "verse_key": "69:39",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167279,
        "resource_id": 31,
        "text": "ainsi que par ce que vous ne voyez pas,"
      }
    ]
  },
  {
    "id": 5363,
    "verse_number": 40,
    "verse_key": "69:40",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167282,
        "resource_id": 31,
        "text": "que ceci [le Coran] est la parole d’un noble Messager , <sup foot_note=232972>1</sup>"
      }
    ]
  },
  {
    "id": 5364,
    "verse_number": 41,
    "verse_key": "69:41",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167284,
        "resource_id": 31,
        "text": "et que ce n’est pas la parole d’un poète; mais vous ne croyez que très peu,"
      }
    ]
  },
  {
    "id": 5365,
    "verse_number": 42,
    "verse_key": "69:42",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167285,
        "resource_id": 31,
        "text": "ni la parole d’un devin, mais vous vous rappelez bien peu."
      }
    ]
  },
  {
    "id": 5366,
    "verse_number": 43,
