    "translations": [
      {
        "id": 167286,
        "resource_id": 31,
        "text": "C’est une révélation du Seigneur de l’Univers."
      }
    ]
  },
  {
    "id": 5367,
    "verse_number": 44,
    "verse_key": "69:44",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167287,
        "resource_id": 31,
        "text": "Et s’il avait forgé quelques paroles qu’ils Nous avait attribuées,"
      }
    ]
  },
  {
    "id": 5368,
    "verse_number": 45,
    "verse_key": "69:45",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167288,
        "resource_id": 31,
        "text": "Nous l’aurions saisi de la main droite,"
      }
    ]
  },
  {
    "id": 5369,
    "verse_number": 46,
    "verse_key": "69:46",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167289,
        "resource_id": 31,
        "text": "ensuite, Nous lui aurions tranché l’aorte."
      }
    ]
  },
  {
    "id": 5370,
    "verse_number": 47,
    "verse_key": "69:47",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167290,
        "resource_id": 31,
        "text": "Et nul d’entre vous n’aurait pu lui servir de rempart."
      }
    ]
  },
  {
    "id": 5371,
    "verse_number": 48,
    "verse_key": "69:48",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167291,
        "resource_id": 31,
        "text": "C’est en vérité un rappel pour les pieux."
      }
    ]
  },
  {
    "id": 5372,
    "verse_number": 49,
    "verse_key": "69:49",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167292,
        "resource_id": 31,
        "text": "Et Nous savons qu’il y a parmi vous qui le traitent de menteur ;"
      }
    ]
  },
  {
    "id": 5373,
    "verse_number": 50,
    "verse_key": "69:50",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 166951,
        "resource_id": 31,
        "text": "mais en vérité, ce sera un sujet de regret pour les mécréants,"
      }
    ]
  },
  {
    "id": 5374,
    "verse_number": 51,
    "verse_key": "69:51",
    "hizb_number": 57,
    "rub_el_hizb_number": 227,
    "ruku_number": 503,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 568,
    "juz_number": 29,
    "translations": [
      {
        "id": 167045,
        "resource_id": 31,
        "text": "c’est là la véritable certitude."
      }
    ]
  },
  {
    "id": 5375,
