    "page_number": 457,
    "juz_number": 23,
    "translations": [
      {
        "id": 165535,
        "resource_id": 31,
        "text": "\"Seigneur, dit [Iblîs,] donne-moi donc un délai, jusqu’au jour où ils seront ressuscités.\""
      }
    ]
  },
  {
    "id": 4050,
    "verse_number": 80,
    "verse_key": "38:80",
    "hizb_number": 46,
    "rub_el_hizb_number": 183,
    "ruku_number": 397,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 457,
    "juz_number": 23,
    "translations": [
      {
        "id": 165539,
        "resource_id": 31,
        "text": "(Allah) dit : \"Tu es de ceux à qui un délai est accordé,"
      }
    ]
  },
  {
    "id": 4051,
    "verse_number": 81,
    "verse_key": "38:81",
    "hizb_number": 46,
    "rub_el_hizb_number": 183,
    "ruku_number": 397,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 457,
    "juz_number": 23,
    "translations": [
      {
        "id": 165548,
        "resource_id": 31,
        "text": "jusqu’au jour de l’Instant bien Connu.\""
      }
    ]
  },
  {
    "id": 4052,
    "verse_number": 82,
    "verse_key": "38:82",
    "hizb_number": 46,
    "rub_el_hizb_number": 183,
    "ruku_number": 397,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 457,
    "juz_number": 23,
    "translations": [
      {
        "id": 165540,
        "resource_id": 31,
        "text": "\"Par Ta puissance ! dit [Satan]. Je les séduirai assurément tous,"
      }
    ]
  },
  {
    "id": 4053,
    "verse_number": 83,
    "verse_key": "38:83",
    "hizb_number": 46,
    "rub_el_hizb_number": 183,
    "ruku_number": 397,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 457,
    "juz_number": 23,
    "translations": [
      {
        "id": 165541,
        "resource_id": 31,
        "text": "sauf Tes serviteurs élus parmi eux.\""
      }
    ]
  },
  {
    "id": 4054,
    "verse_number": 84,
    "verse_key": "38:84",
    "hizb_number": 46,
    "rub_el_hizb_number": 183,
    "ruku_number": 397,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 458,
    "juz_number": 23,
    "translations": [
      {
        "id": 165542,
        "resource_id": 31,
        "text": "(Allah) dit : \"En vérité, et c’est la vérité que je dis,"
      }
    ]
  },
  {
    "id": 4055,
    "verse_number": 85,
    "verse_key": "38:85",
    "hizb_number": 46,
    "rub_el_hizb_number": 183,
    "ruku_number": 397,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 458,
    "juz_number": 23,
    "translations": [
      {
        "id": 165543,
        "resource_id": 31,
        "text": "J’emplirai certainement l’Enfer de toi et de tous ceux d’entre eux qui te suivront.\""
      }
    ]
  },
  {
    "id": 4056,
    "verse_number": 86,
    "verse_key": "38:86",
    "hizb_number": 46,
    "rub_el_hizb_number": 183,
    "ruku_number": 397,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 458,
    "juz_number": 23,
    "translations": [
      {
        "id": 165544,
        "resource_id": 31,
        "text": "Dis : \"Pour cela, je ne vous demande aucun salaire; et je ne suis pas un imposteur."
      }
    ]
  },
  {
    "id": 4057,
    "verse_number": 87,
    "verse_key": "38:87",
    "hizb_number": 46,
    "rub_el_hizb_number": 183,
    "ruku_number": 397,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 458,
    "juz_number": 23,
    "translations": [
      {
        "id": 165545,
        "resource_id": 31,
        "text": "Ceci [le Coran] n’est qu’un rappel à l’Univers."
      }
    ]
  },
  {
    "id": 4058,
    "verse_number": 88,
    "verse_key": "38:88",
    "hizb_number": 46,
    "rub_el_hizb_number": 183,
    "ruku_number": 397,
    "manzil_number": 6,
    "sajdah_number": null,
    "page_number": 458,
    "juz_number": 23,
    "translations": [
      {
        "id": 165546,
        "resource_id": 31,
        "text": "Et certainement vous en aurez des nouvelles bientôt !\"."
      }
    ]
  }
]