[
  {
    "id": 5910,
    "verse_number": 1,
    "verse_key": "85:1",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168083,
        "resource_id": 31,
        "text": "Par le ciel aux constellations !"
      }
    ]
  },
  {
    "id": 5911,
    "verse_number": 2,
    "verse_key": "85:2",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168067,
        "resource_id": 31,
        "text": "et par le jour promis !"
      }
    ]
  },
  {
    "id": 5912,
    "verse_number": 3,
    "verse_key": "85:3",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168069,
        "resource_id": 31,
        "text": "et par le témoin <sup foot_note=233077>1</sup> et ce dont on témoigne !"
      }
    ]
  },
  {
    "id": 5913,
    "verse_number": 4,
    "verse_key": "85:4",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168071,
        "resource_id": 31,
        "text": "Périssent les gens ceux [qui creusèrent ] des fossés (Al Ukhdûd) <sup foot_note=233078>1</sup>,"
      }
    ]
  },
  {
    "id": 5914,
    "verse_number": 5,
    "verse_key": "85:5",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168073,
        "resource_id": 31,
        "text": "au feu plein de combustible,"
      }
    ]
  },
  {
    "id": 5915,
    "verse_number": 6,
    "verse_key": "85:6",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168075,
        "resource_id": 31,
        "text": "cependant qu’ils étaient assis tout autour,"
      }
    ]
  },
  {
    "id": 5916,
    "verse_number": 7,
    "verse_key": "85:7",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168077,
        "resource_id": 31,
        "text": "ils étaient ainsi témoins de ce qu’ils faisaient des croyants,"
      }
    ]
  },
  {
    "id": 5917,
    "verse_number": 8,
    "verse_key": "85:8",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168079,
        "resource_id": 31,
        "text": "à qui ils ne leur reprochaient que d’avoir cru en Allah, le Puissant, le Digne de louange,"
      }
    ]
  },
  {
    "id": 5918,
    "verse_number": 9,
    "verse_key": "85:9",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168081,
        "resource_id": 31,
        "text": "Auquel appartient la royauté des cieux et de la Terre. Allah est témoin de toute chose."
      }
    ]
  },
  {
    "id": 5919,
    "verse_number": 10,
    "verse_key": "85:10",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168054,
        "resource_id": 31,
        "text": "Ceux qui font subir des épreuves aux croyants et aux croyantes, puis ne se repentent pas, auront le châtiment de l’Enfer et le supplice du feu."
      }
    ]
  },
  {
    "id": 5920,
    "verse_number": 11,
    "verse_key": "85:11",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168055,
        "resource_id": 31,
        "text": "Ceux qui croient et accomplissent les bonnes œuvres auront des Jardins sous lesquels coulent les ruisseaux. Cela est le grand succès."
      }
    ]
  },
  {
    "id": 5921,
    "verse_number": 12,
    "verse_key": "85:12",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168056,
        "resource_id": 31,
        "text": "La riposte de ton Seigneur est redoutable."
      }
    ]
  },
  {
    "id": 5922,
    "verse_number": 13,
    "verse_key": "85:13",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168064,
        "resource_id": 31,
        "text": "C’est Lui, certes, qui commence (la création) et la refait."
      }
    ]
  },
  {
    "id": 5923,
    "verse_number": 14,
    "verse_key": "85:14",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168057,
        "resource_id": 31,
        "text": "Et c’est Lui le Pardonneur, le Tout Affectueux,"
      }
    ]
  },
  {
    "id": 5924,
    "verse_number": 15,
    "verse_key": "85:15",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168058,
        "resource_id": 31,
        "text": "Le Maître du Trône, le Tout Glorieux,"
      }
    ]
  },
  {
    "id": 5925,
    "verse_number": 16,
    "verse_key": "85:16",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168059,
        "resource_id": 31,
        "text": "Il réalise parfaitement tout ce qu’Il veut."
      }
    ]
  },
  {
    "id": 5926,
    "verse_number": 17,
    "verse_key": "85:17",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168060,
        "resource_id": 31,
        "text": "T’est-il parvenu le récit des armées,"
      }
    ]
  },
  {
    "id": 5927,
    "verse_number": 18,
    "verse_key": "85:18",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168061,
        "resource_id": 31,
        "text": "de Pharaon, et de Thamûd ?"
      }
    ]
  },
  {
    "id": 5928,
    "verse_number": 19,
    "verse_key": "85:19",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168062,
        "resource_id": 31,
        "text": "Mais ceux qui ne croient pas persistent à démentir,"
      }
    ]
  },
  {
    "id": 5929,
    "verse_number": 20,
    "verse_key": "85:20",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 168065,
        "resource_id": 31,
        "text": "alors qu’Allah, derrière eux, les cerne de toutes parts."
      }
    ]
  },
  {
    "id": 5930,
    "verse_number": 21,
    "verse_key": "85:21",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 166835,
        "resource_id": 31,
        "text": "Mais c’est plutôt un Coran glorifié."
      }
    ]
  },
  {
    "id": 5931,
    "verse_number": 22,
    "verse_key": "85:22",
    "hizb_number": 59,
    "rub_el_hizb_number": 236,
    "ruku_number": 529,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 590,
    "juz_number": 30,
    "translations": [
      {
        "id": 167418,
        "resource_id": 31,
        "text": "préservé sur une Tablette (auprès d’Allah)."
      }
    ]
  }
]