[
  {
    "id": 5849,
    "verse_number": 1,
    "verse_key": "83:1",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 164802,
        "resource_id": 31,
        "text": "Malheur aux fraudeurs,"
      }
    ]
  },
  {
    "id": 5850,
    "verse_number": 2,
    "verse_key": "83:2",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 167989,
        "resource_id": 31,
        "text": "qui, lorsqu’ils font mesurer pour eux-mêmes exigent la pleine mesure,"
      }
    ]
  },
  {
    "id": 5851,
    "verse_number": 3,
    "verse_key": "83:3",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 168002,
        "resource_id": 31,
        "text": "et qui lorsqu’eux-mêmes mesurent ou pèsent pour les autres, [leur] causent perte."
      }
    ]
  },
  {
    "id": 5852,
    "verse_number": 4,
    "verse_key": "83:4",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 168004,
        "resource_id": 31,
        "text": "Ceux-là ne pensent-ils pas qu’ils seront ressuscités,"
      }
    ]
  },
  {
    "id": 5853,
    "verse_number": 5,
    "verse_key": "83:5",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 165069,
        "resource_id": 31,
        "text": "en un jour terrible,"
      }
    ]
  },
  {
    "id": 5854,
    "verse_number": 6,
    "verse_key": "83:6",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 587,
    "juz_number": 30,
    "translations": [
      {
        "id": 168008,
        "resource_id": 31,
        "text": "le jour où les gens se tiendront debout devant le Seigneur de l’Univers ?"
      }
    ]
  },
  {
    "id": 5855,
    "verse_number": 7,
    "verse_key": "83:7",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 168011,
        "resource_id": 31,
        "text": "Non...! Mais en vérité le livre des libertins sera dans le Sijjîn."
      }
    ]
  },
  {
    "id": 5856,
    "verse_number": 8,
    "verse_key": "83:8",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 168017,
        "resource_id": 31,
        "text": "et qui te dira ce qu’est le Sijjîn ? -"
      }
    ]
  },
  {
    "id": 5857,
    "verse_number": 9,
    "verse_key": "83:9",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 168013,
        "resource_id": 31,
        "text": "un livre déjà cacheté (achevé)."
      }
    ]
  },
  {
    "id": 5858,
    "verse_number": 10,
    "verse_key": "83:10",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167971,
        "resource_id": 31,
        "text": "Malheur, ce jour-là, aux négateurs,"
      }
    ]
  },
  {
    "id": 5859,
    "verse_number": 11,
    "verse_key": "83:11",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167981,
        "resource_id": 31,
        "text": "qui démentent le jour de la Rétribution."
      }
    ]
  },
  {
    "id": 5860,
    "verse_number": 12,
    "verse_key": "83:12",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167972,
        "resource_id": 31,
        "text": "Or, ne le dément que tout transgresseur, pécheur :"
      }
    ]
  },
  {
    "id": 5861,
    "verse_number": 13,
    "verse_key": "83:13",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167973,
        "resource_id": 31,
        "text": "qui, lorsque Nos versets lui sont récités, dit : \"[Ce sont] des contes d’anciens !\""
      }
    ]
  },
  {
    "id": 5862,
    "verse_number": 14,
    "verse_key": "83:14",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167974,
        "resource_id": 31,
        "text": "Pas du tout, mais ce qu’ils ont accompli couvre leurs cœurs <sup foot_note=233072>1</sup>."
      }
    ]
  },
  {
    "id": 5863,
    "verse_number": 15,
    "verse_key": "83:15",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167975,
        "resource_id": 31,
        "text": "Qu’ils prennent garde ! En vérité ce jour-là un voile les empêchera de voir leur Seigneur,"
      }
    ]
  },
  {
    "id": 5864,
    "verse_number": 16,
    "verse_key": "83:16",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167976,
        "resource_id": 31,
        "text": "ensuite, ils brûleront certes, dans la Fournaise;"
      }
    ]
  },
  {
    "id": 5865,
    "verse_number": 17,
    "verse_key": "83:17",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167977,
        "resource_id": 31,
        "text": "on [leur] dira alors : \"Voilà ce que vous traitiez de mensonge !\""
      }
    ]
  },
  {
    "id": 5866,
    "verse_number": 18,
    "verse_key": "83:18",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167978,
        "resource_id": 31,
        "text": "Qu’ils prennent garde ! Le livre des bons sera dans 'Illiyûn -"
      }
    ]
  },
  {
    "id": 5867,
    "verse_number": 19,
    "verse_key": "83:19",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167979,
        "resource_id": 31,
        "text": "et qui te dira ce qu’est 'Illiyûn ?"
      }
    ]
  },
  {
    "id": 5868,
    "verse_number": 20,
    "verse_key": "83:20",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167982,
        "resource_id": 31,
        "text": "un livre cacheté !"
      }
    ]
  },
  {
    "id": 5869,
    "verse_number": 21,
    "verse_key": "83:21",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167983,
        "resource_id": 31,
        "text": "Les rapprochés (d’Allah : les Anges) en témoignent."
      }
    ]
  },
  {
    "id": 5870,
    "verse_number": 22,
    "verse_key": "83:22",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167984,
        "resource_id": 31,
        "text": "Les bons seront dans [un jardin] de délice,"
      }
    ]
  },
  {
    "id": 5871,
    "verse_number": 23,
    "verse_key": "83:23",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167985,
        "resource_id": 31,
        "text": "sur les divans, ils regardent."
      }
    ]
  },
  {
    "id": 5872,
    "verse_number": 24,
    "verse_key": "83:24",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167986,
        "resource_id": 31,
        "text": "Tu reconnaîtras sur leurs visages, l’éclat de la félicité."
      }
    ]
  },
  {
    "id": 5873,
    "verse_number": 25,
    "verse_key": "83:25",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167987,
        "resource_id": 31,
        "text": "On leur sert à boire un nectar pur, cacheté,"
      }
    ]
  },
  {
    "id": 5874,
    "verse_number": 26,
    "verse_key": "83:26",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167988,
        "resource_id": 31,
        "text": "laissant un arrière-goût de musc. Que ceux qui la convoitent entrent en compétition [pour l’acquérir]"
      }
    ]
  },
  {
    "id": 5875,
    "verse_number": 27,
    "verse_key": "83:27",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167993,
        "resource_id": 31,
        "text": "Il est mélangé à la boisson de Tasnîm <sup foot_note=233074>1</sup>,"
      }
    ]
  },
  {
    "id": 5876,
    "verse_number": 28,
    "verse_key": "83:28",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167990,
        "resource_id": 31,
        "text": "source dont les rapprochés boivent."
      }
    ]
  },
  {
    "id": 5877,
    "verse_number": 29,
    "verse_key": "83:29",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167991,
        "resource_id": 31,
        "text": "Les criminels riaient de ceux qui croyaient,"
      }
    ]
  },
  {
    "id": 5878,
    "verse_number": 30,
    "verse_key": "83:30",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167994,
        "resource_id": 31,
        "text": "et, passant près d’eux, ils se faisaient des œillades,"
      }
    ]
  },
  {
    "id": 5879,
    "verse_number": 31,
    "verse_key": "83:31",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167995,
        "resource_id": 31,
        "text": "et, retournant dans leurs familles, ils retournaient en plaisantant,"
      }
    ]
  },
  {
    "id": 5880,
    "verse_number": 32,
    "verse_key": "83:32",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167996,
        "resource_id": 31,
        "text": "et les voyant, ils disaient : \"Ce sont vraiment ceux-là les égarés.\""
      }
    ]
  },
  {
    "id": 5881,
    "verse_number": 33,
    "verse_key": "83:33",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167997,
        "resource_id": 31,
        "text": "Or, ils n’ont pas été envoyés pour être leurs gardiens."
      }
    ]
  },
  {
    "id": 5882,
    "verse_number": 34,
    "verse_key": "83:34",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 588,
    "juz_number": 30,
    "translations": [
      {
        "id": 167998,
        "resource_id": 31,
        "text": "Aujourd’hui, donc, ce sont ceux qui ont cru qui rient des infidèles."
      }
    ]
  },
  {
    "id": 5883,
    "verse_number": 35,
    "verse_key": "83:35",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 589,
    "juz_number": 30,
    "translations": [
      {
        "id": 167999,
        "resource_id": 31,
        "text": "sur les divans, ils regardent."
      }
    ]
  },
  {
    "id": 5884,
    "verse_number": 36,
    "verse_key": "83:36",
    "hizb_number": 59,
    "rub_el_hizb_number": 235,
    "ruku_number": 527,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 589,
    "juz_number": 30,
    "translations": [
      {
        "id": 168000,
        "resource_id": 31,
        "text": "Est-ce que les infidèles ont eu la récompense de ce qu’ils faisaient ?"
      }
    ]
  }
]