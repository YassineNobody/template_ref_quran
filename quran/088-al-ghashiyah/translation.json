[
  {
    "id": 5968,
    "verse_number": 1,
    "verse_key": "88:1",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168163,
        "resource_id": 31,
        "text": "T’est-il parvenu le récit de l’enveloppante ? <sup foot_note=233083>1</sup>"
      }
    ]
  },
  {
    "id": 5969,
    "verse_number": 2,
    "verse_key": "88:2",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168147,
        "resource_id": 31,
        "text": "Ce jour-là, il y aura des visages humiliés,"
      }
    ]
  },
  {
    "id": 5970,
    "verse_number": 3,
    "verse_key": "88:3",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168155,
        "resource_id": 31,
        "text": "préoccupés, harassés."
      }
    ]
  },
  {
    "id": 5971,
    "verse_number": 4,
    "verse_key": "88:4",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168156,
        "resource_id": 31,
        "text": "Ils brûleront dans un Feu ardent,"
      }
    ]
  },
  {
    "id": 5972,
    "verse_number": 5,
    "verse_key": "88:5",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168157,
        "resource_id": 31,
        "text": "et seront abreuvés d’une source bouillante."
      }
    ]
  },
  {
    "id": 5973,
    "verse_number": 6,
    "verse_key": "88:6",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168158,
        "resource_id": 31,
        "text": "Il n’y aura pour eux d’autre nourriture que des plantes épineuses [Dharî'],"
      }
    ]
  },
  {
    "id": 5974,
    "verse_number": 7,
    "verse_key": "88:7",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168159,
        "resource_id": 31,
        "text": "qui n’engraisse, ni n’apaise la faim."
      }
    ]
  },
  {
    "id": 5975,
    "verse_number": 8,
    "verse_key": "88:8",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168160,
        "resource_id": 31,
        "text": "Ce jour-là, il y aura des visages épanouis,"
      }
    ]
  },
  {
    "id": 5976,
    "verse_number": 9,
    "verse_key": "88:9",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168161,
        "resource_id": 31,
        "text": "contents de leurs efforts,"
      }
    ]
  },
  {
    "id": 5977,
    "verse_number": 10,
    "verse_key": "88:10",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168138,
        "resource_id": 31,
        "text": "dans un haut Jardin,"
      }
    ]
  },
  {
    "id": 5978,
    "verse_number": 11,
    "verse_key": "88:11",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168139,
        "resource_id": 31,
        "text": "où ils n’entendent aucune futilité."
      }
    ]
  },
  {
    "id": 5979,
    "verse_number": 12,
    "verse_key": "88:12",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168140,
        "resource_id": 31,
        "text": "Là, il y aura une source coulante."
      }
    ]
  },
  {
    "id": 5980,
    "verse_number": 13,
    "verse_key": "88:13",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168141,
        "resource_id": 31,
        "text": "Là, des divans élevés."
      }
    ]
  },
  {
    "id": 5981,
    "verse_number": 14,
    "verse_key": "88:14",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168142,
        "resource_id": 31,
        "text": "et des coupes posées."
      }
    ]
  },
  {
    "id": 5982,
    "verse_number": 15,
    "verse_key": "88:15",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168143,
        "resource_id": 31,
        "text": "et des coussins rangés."
      }
    ]
  },
  {
    "id": 5983,
    "verse_number": 16,
    "verse_key": "88:16",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168144,
        "resource_id": 31,
        "text": "et des tapis étalés."
      }
    ]
  },
  {
    "id": 5984,
    "verse_number": 17,
    "verse_key": "88:17",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168145,
        "resource_id": 31,
        "text": "Ne considèrent-ils donc pas les chameaux, comment ils ont été créés,"
      }
    ]
  },
  {
    "id": 5985,
    "verse_number": 18,
    "verse_key": "88:18",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 164778,
        "resource_id": 31,
        "text": "et le ciel comment il est élevé,"
      }
    ]
  },
  {
    "id": 5986,
    "verse_number": 19,
    "verse_key": "88:19",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168146,
        "resource_id": 31,
        "text": "et les montagnes comment elles sont dressées."
      }
    ]
  },
  {
    "id": 5987,
    "verse_number": 20,
    "verse_key": "88:20",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168148,
        "resource_id": 31,
        "text": "et la terre comment elle est nivelée ?"
      }
    ]
  },
  {
    "id": 5988,
    "verse_number": 21,
    "verse_key": "88:21",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168149,
        "resource_id": 31,
        "text": "Eh bien, rappelle ! Tu n’es qu’un rappeleur,"
      }
    ]
  },
  {
    "id": 5989,
    "verse_number": 22,
    "verse_key": "88:22",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168150,
        "resource_id": 31,
        "text": "et tu n’es pas un dominateur sur eux."
      }
    ]
  },
  {
    "id": 5990,
    "verse_number": 23,
    "verse_key": "88:23",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168151,
        "resource_id": 31,
        "text": "Sauf celui qui tourne le dos et ne croit pas,"
      }
    ]
  },
  {
    "id": 5991,
    "verse_number": 24,
    "verse_key": "88:24",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168152,
        "resource_id": 31,
        "text": "alors Allah le châtiera du plus grand châtiment."
      }
    ]
  },
  {
    "id": 5992,
    "verse_number": 25,
    "verse_key": "88:25",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168153,
        "resource_id": 31,
        "text": "Vers Nous est leur retour."
      }
    ]
  },
  {
    "id": 5993,
    "verse_number": 26,
    "verse_key": "88:26",
    "hizb_number": 60,
    "rub_el_hizb_number": 237,
    "ruku_number": 532,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 592,
    "juz_number": 30,
    "translations": [
      {
        "id": 168154,
        "resource_id": 31,
        "text": "Ensuite, c’est à Nous de leur demander compte."
      }
    ]
  }
]