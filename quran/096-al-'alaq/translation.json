[
  {
    "id": 6107,
    "verse_number": 1,
    "verse_key": "96:1",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 165038,
        "resource_id": 31,
        "text": "Lis, au nom de ton Seigneur qui a créé,"
      }
    ]
  },
  {
    "id": 6108,
    "verse_number": 2,
    "verse_key": "96:2",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 165077,
        "resource_id": 31,
        "text": "qui a créé l’homme d’une adhérence."
      }
    ]
  },
  {
    "id": 6109,
    "verse_number": 3,
    "verse_key": "96:3",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 169994,
        "resource_id": 31,
        "text": "Lis! Ton Seigneur est le Très Noble,"
      }
    ]
  },
  {
    "id": 6110,
    "verse_number": 4,
    "verse_key": "96:4",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168370,
        "resource_id": 31,
        "text": "qui a enseigné par la plume [le calame],"
      }
    ]
  },
  {
    "id": 6111,
    "verse_number": 5,
    "verse_key": "96:5",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168372,
        "resource_id": 31,
        "text": "a enseigné à l’homme ce qu’il ne savait pas."
      }
    ]
  },
  {
    "id": 6112,
    "verse_number": 6,
    "verse_key": "96:6",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168374,
        "resource_id": 31,
        "text": "Prenez-garde ! Vraiment l’homme devient rebelle,"
      }
    ]
  },
  {
    "id": 6113,
    "verse_number": 7,
    "verse_key": "96:7",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168379,
        "resource_id": 31,
        "text": "dès qu’il estime qu’il peut se suffire à lui-même (à cause de sa richesse)."
      }
    ]
  },
  {
    "id": 6114,
    "verse_number": 8,
    "verse_key": "96:8",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168381,
        "resource_id": 31,
        "text": "Mais, c’est vers ton Seigneur qu’est le retour."
      }
    ]
  },
  {
    "id": 6115,
    "verse_number": 9,
    "verse_key": "96:9",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168383,
        "resource_id": 31,
        "text": "As-tu vu celui qui interdit."
      }
    ]
  },
  {
    "id": 6116,
    "verse_number": 10,
    "verse_key": "96:10",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168480,
        "resource_id": 31,
        "text": "à un serviteur d’Allah (Mohammed) de célébrer la prière (As-Salât) ?"
      }
    ]
  },
  {
    "id": 6117,
    "verse_number": 11,
    "verse_key": "96:11",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168387,
        "resource_id": 31,
        "text": "Vois-tu s’il est sur la bonne voie,"
      }
    ]
  },
  {
    "id": 6118,
    "verse_number": 12,
    "verse_key": "96:12",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168385,
        "resource_id": 31,
        "text": "ou s’il ordonne la piété?"
      }
    ]
  },
  {
    "id": 6119,
    "verse_number": 13,
    "verse_key": "96:13",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168360,
        "resource_id": 31,
        "text": "Vois-tu s’il dément et tourne le dos ?"
      }
    ]
  },
  {
    "id": 6120,
    "verse_number": 14,
    "verse_key": "96:14",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168368,
        "resource_id": 31,
        "text": "Ne sait-il pas que vraiment Allah voit ?"
      }
    ]
  },
  {
    "id": 6121,
    "verse_number": 15,
    "verse_key": "96:15",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168361,
        "resource_id": 31,
        "text": "Mais non ! S’il ne cesse pas, Nous le saisirons certes, par le toupet,"
      }
    ]
  },
  {
    "id": 6122,
    "verse_number": 16,
    "verse_key": "96:16",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168362,
        "resource_id": 31,
        "text": "le toupet d’un menteur, d’un pécheur."
      }
    ]
  },
  {
    "id": 6123,
    "verse_number": 17,
    "verse_key": "96:17",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168366,
        "resource_id": 31,
        "text": "Qu’il appelle donc son assemblée."
      }
    ]
  },
  {
    "id": 6124,
    "verse_number": 18,
    "verse_key": "96:18",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": null,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168363,
        "resource_id": 31,
        "text": "Nous appellerons les gardiens (de l’Enfer)."
      }
    ]
  },
  {
    "id": 6125,
    "verse_number": 19,
    "verse_key": "96:19",
    "hizb_number": 60,
    "rub_el_hizb_number": 239,
    "ruku_number": 540,
    "manzil_number": 7,
    "sajdah_number": 14,
    "page_number": 597,
    "juz_number": 30,
    "translations": [
      {
        "id": 168364,
        "resource_id": 31,
        "text": "Non ! Ne lui obéis pas; mais prosterne-toi et rapproche-toi <sup foot_note=233110>1</sup>."
      }
    ]
  }
]